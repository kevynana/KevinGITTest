-SET &&DB='H7METX';
-SET &&PNR = 'GZENLS';
-SET &&DATE = '20140129';
-RUN
TABLE FILE &&DB
PRINT HTL_KEY
BY HTL_KEY NOPRINT
WHERE PNR_LOCATOR EQ '&&PNR'
-* WHERE CHKOUT_DATE EQ '20140522'
-* WHERE INVOICE_DATE EQ '&&DATE'
-* WHERE HTL_KEY CONTAINS 'VAX'
WHERE INVOICE_DATE LT '20140814'
ON TABLE HOLD
END
-RUN
MODIFY FILE &&DB
FIXFORM FROM HOLD
MATCH HTL_KEY
ON MATCH DELETE
ON NOMATCH REJECT
DATA ON HOLD
END