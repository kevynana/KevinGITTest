
TABLE FILE BRPTINST
BY BREAK_LEVELS  
WHERE INST_KEY EQ &&IKEY
ON TABLE SAVE  
END
-RUN

-READ SAVE &BL.18.
-SET &LEN = ARGLEN(60, '&BL.EVAL', 'I2');
-SET &NBRLVL = &LEN / 3;
-CLOSE SAVE


-SET &NR1 = 1 + &NBRLVL.EVAL;
-SET &RN1 = 'N' || '&NR1.EVAL';
-SET &NR2 = 2 + &NBRLVL.EVAL;
-SET &RN2 = 'N' || '&NR2.EVAL';
-SET &NR3 = 3 + &NBRLVL.EVAL;
-SET &RN3 = 'N' || '&NR3.EVAL';
-SET &NR4 = 4 + &NBRLVL.EVAL;
-SET &RN4 = 'N' || '&NR4.EVAL';
 
 
 
