-SET &AVGDR_CUR = 'AVERAGE,DAILY,RATE, (' | &&CURRENCY || ')';



TABLE FILE RPTHOLD
HEADING
"Client: <6 <ROLL_NAME <37 <RN"
"Contact: <6 <ROLL_CONTACT <30 <LVL <85 &&DTDESC"
"Phone: <6 <RPHONE <85 &FMM/&FMD/&FMY - &TOM/&TOD/&TOY" 
" "
" "
-*
-*******************************************************************
-* Sum statement with fields passed from the user profile database
-*******************************************************************
-*
SUM 
NNROOMS AS 'NUMBER,NIGHTS'
NBR_ROOMS AS 'NUMBER,ROOMS'
AMT_TOTALX/D16.2CSM AS '&TAMT_CUR.EVAL'
COMPUTE AVGDR/D16.2CSM = IF AMT_TOTALX LT 0 AND NNROOMS LT 0
 THEN ((AMT_TOTALX/NNROOMS) * (-1)) 
 ELSE AMT_TOTALX/NNROOMS; AS '&AVGDR_CUR.EVAL'

-GOTO CONTINUE;
-STEP2


TABLE FILE RPTHOLD
HEADING
"Client: <6 <ROLL_NAME <37 <RN"
"Contact: <6 <ROLL_CONTACT <30 <LVL <85 &&DTDESC"
"Phone: <6 <RPHONE <85 &FMM/&FMD/&FMY - &TOM/&TOD/&TOY" 
" "
" "
-*
-*******************************************************************
-* Sum statement with fields passed from the user profile database
-*******************************************************************
-*
SUM 
NNROOMS AS 'NUMBER,NIGHTS'
NBR_ROOMS AS 'NUMBER,ROOMS'
NEW_TOTAL_AMT/D12.2CSM AS 'TOTAL,AMOUNT'
COMPUTE AVGDR/D12.2CSM = IF NEW_TOTAL_AMT LT 0 AND NNROOMS LT 0
 THEN ((NEW_TOTAL_AMT/NNROOMS) * (-1)) 
 ELSE NEW_TOTAL_AMT/NNROOMS; AS 'AVERAGE,DAILY,RATE'




-CONTINUE;

