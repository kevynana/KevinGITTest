-INCLUDE SETECHO

SET ASNAMES = ON


TABLE FILE BAG_FEE
PRINT AIRLINE
IF AIRLINE EQ BA
ON TABLE HOLD
END
-RUN


MODIFY FILE BAG_FEE
FIXFORM FROM HOLD
MATCH AIRLINE
ON MATCH COMPUTE BAG1FEE=0.00;
ON MATCH COMPUTE BAG2FEE=51.00;
ON MATCH UPDATE BAG1FEE

ON MATCH UPDATE BAG2FEE
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN
