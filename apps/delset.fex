JOIN SET_KEY IN RPT_INST TO SET_KEY IN RPT_SET
END

TABLE FILE RPT_INST
PRINT USER_NAME SET_NAME
BY INST_KEY
WHERE ROLLUP_CODE EQ 'PRIME-R'
WHERE SET_NAME EQ 'PRIME Monthly Member Travel Summaries' OR
                  'PRIME Monthly Travel Summaries';
WHERE USER_NAME EQ 'andreaa'
ON TABLE HOLD
END

MODIFY FILE RPT_INST
FIXFORM FROM HOLD
MATCH INST_KEY
ON MATCH DELETE 
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN









