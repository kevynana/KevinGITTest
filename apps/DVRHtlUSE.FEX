-SET &Q0  = &DATAPATH | 'H0' || &&COMPNY || '.FOC AS HR_50';
-SET &Q1  = &DATAPATH | 'H1' || &&COMPNY || '.FOC AS HR_50';
-SET &Q2  = &DATAPATH | 'H2' || &&COMPNY || '.FOC AS HR_50';
-SET &Q3  = &DATAPATH | 'H3' || &&COMPNY || '.FOC AS HR_50';
-SET &Q4  = &DATAPATH | 'H4' || &&COMPNY || '.FOC AS HR_50';
-SET &Q5  = &DATAPATH | 'H5' || &&COMPNY || '.FOC AS HR_50';
-SET &Q6  = &DATAPATH | 'H6' || &&COMPNY || '.FOC AS HR_50';
-SET &Q7  = &DATAPATH | 'H7' || &&COMPNY || '.FOC AS HR_50';
-SET &Q8  = &DATAPATH | 'H8' || &&COMPNY || '.FOC AS HR_50';
