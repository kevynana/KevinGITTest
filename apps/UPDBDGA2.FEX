DEFINE FILE A1BDGA
TEST/A1 = EDIT(LEVEL2, '9$$$$$$$$$$$$$$$');
END

TABLE FILE A1BDGA
PRINT AIR_KEY LEVEL2 AS 'LEVEL3' LEVEL3 AS 'LEVEL2'
WHERE LEVEL1 EQ 'MEETING'
WHERE TEST EQ '0' OR '1' OR '2' OR '3' OR '4' OR '5' OR '6' OR '7' OR '8' OR '9'
BY AIR_KEY NOPRINT
ON TABLE HOLD AS DEBHOLD
END
-RUN

USE CLEAR *
-RUN

MODIFY FILE A0BDGA
FIXFORM FROM DEBHOLD
MATCH AIR_KEY
ON MATCH UPDATE LEVEL2
ON MATCH UPDATE LEVEL3
ON NOMATCH REJECT
DATA ON DEBHOLD
END
