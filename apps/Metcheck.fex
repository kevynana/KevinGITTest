-SET &&CAR_DB  = IF &&QTR_EN EQ ' ' THEN &&CNDATA ELSE &&CQDATA ;
-SET &&HTL_DB  = IF &&QTR_EN EQ ' ' THEN &&HNDATA ELSE &&HQDATA ;
-*==============================================================================
-*             CHECK AIR DATA (QUERY SEG DATABASE)
-*==============================================================================
DEFINE FILE &&SEG_DB1
XL01/I4    = IF AIR_KEY NE ' ' THEN 1 ELSE 0 ;
XL02/P9.2C = IF AIR_KEY NE ' ' THEN SEG_AMT ELSE 0 ;
XL03/P9.2C = IF AIR_KEY NE ' ' THEN SEG_TAX ELSE 0 ;
XL04/P9.2C = XL02 + XL03 ;
XL05/I4 = IF TRN_DATE GE '&&FMDATE' AND (TRN_DATE LE '&&TODATE') THEN 1 ELSE 0;
XL06/A4 = IF XL01 GT 0 THEN 'OK' ELSE 'FAIL' ;
XL07/A25= '&&RPTNAM' ;
XL08/A1 = '&&RCYCLE' ;
XL09/YYMD = &YYMD ;
XL10/A8 = '&&ROLL';
XL11/A8 = '&&FMDATE';
XL12/I8 = EDIT(XL11);
XL13/I8YYMD=XL12 ;
XL14/YYMD=XL13;
XL15/A8 = '&&TODATE';
XL16/I8 = EDIT(XL15);
XL17/I8YYMD=XL16 ;
XL18/YYMD=XL17;
END
TABLE FILE &&SEG_DB1
SUM
  XL10         AS 'ROLLUP'
  XL07         AS 'RNAME'
  XL08         AS 'FREQ'
  XL09         AS 'RDATE'
  XL01         AS 'SEGCNT'
  XL02         AS 'SEGDOL'
  XL14         AS 'SEGFM'
  XL18         AS 'SEGTO'
  MAX.TRN_DATE AS 'SEGMX'
  XL06         AS 'SEGST'
IF TRN_DATE GE '&&FMDATE'
IF TRN_DATE LE '&&TODATE'
ON TABLE HOLD
END
-RUN
MODIFY FILE LOADCHK
FIXFORM FROM HOLD
MATCH ROLLUP
ON NOMATCH INCLUDE
DATA ON HOLD
END
