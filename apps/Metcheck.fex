DEFINE FILE LOADCHK
AA01/A8  = ROLLUP ;
AA02/A25 = RNAME ;
AA03/A10 = IF FREQ EQ 'W' THEN 'WEEKLY' ELSE 'MONTHLY' ;
AA04/A55 = AA01||'----->'||AA02||'  ('||AA03||')' ;
AA05/A4 = IF SEGCNT NE 0 THEN 'OK' ELSE 'FAIL';
AA06/A4 = IF CARCNT NE 0 THEN 'OK' ELSE 'FAIL';
AA07/A4 = IF HTLCNT NE 0 THEN 'OK' ELSE 'FAIL';
AA08/A12=AA05||AA06||AA07;
END
