-INCLUDE SETECHO

SET ASNAMES = ON
-RUN

-IF &&CURRENCY EQ 'USD' THEN GOTO SKIPTHIS;

-SET &&SUBHEAD = <NAMEC;
-SET &FPAID_CUR = 'FARE PAID, (' | &&CURRENCY || ')';
-SET &GBASE_CUR= 'BASE,FARE, (' | &&CURRENCY || ')';
-SET &GAVGB_CUR ='AVG,BASE,COST, (' | &&CURRENCY || ')';
-SET &GAVGT_CUR ='AVG,TKT,COST, (' | &&CURRENCY || ')';
-SET &&S_SUBJ3='FARE_BASE1/D16.2CS AS ' | '''&GBASE_CUR.EVAL''' ;
-SET &&S_SUBJ4='PAID_FARE1/D16.2S AS ' | '''&FPAID_CUR.EVAL''';
-SET &&S_SUBJ5='COMPUTE AVGBASE/D16.2CS = IF ((NET_TKT_CNT LT 1) AND';
-SET &&S_SUBJ6='(NET_TKT_CNT GT (-1))) THEN 0 ELSE';
-SET &&S_SUBJ7='(FARE_BASE1/NET_TKT_CNT); AS ' | '''&GAVGB_CUR.EVAL''' ;
-SET &&S_SUBJ8='COMPUTE AVGTKT/D16.2CS = IF ((NET_TKT_CNT LT 1) AND';
-SET &&S_SUBJ9='(NET_TKT_CNT GT (-1))) THEN 0 ELSE';
-SET &&S_SUBJ10='(PAID_FARE1/NET_TKT_CNT); AS ' | '''&GAVGT_CUR.EVAL''' ;





-SKIPTHIS;

TABLE FILE SATNIT2
SUM FARE_PAID/D12.2CS AS 'TFARE'
    MRK_TKT_CNT/D8CS AS 'TMKT'
    NET_COMM/D12.2CS AS 'NCOMM'
    BASE_FARE/D12.CS AS 'TBASE'
    PAID_FARE1/D16.2CS AS 'GTFARE'
    FARE_BASE1/D16.2CS AS 'GTBASE'
BY LEVEL_DESC    
ON TABLE HOLD AS HOLDONE
END
-RUN

TABLE FILE SATNIT2
PRINT
  AGENT_NUM
  AIRLINE 	
  AIRLINE_NAME
  BASE_FARE	
  CLASS	
  CLASS_CAT
  EMB_APT_NM
