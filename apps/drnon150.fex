DEFINE FILE &&EXTRACT
APP_FLAG/A30 = IF REFUSE_CODE EQ 'A' OR 'R' THEN 'PREFERRED'
               ELSE 'OTHER';
CITY_ST/A35 = PROP_CITY||(', '| PROP_STATE);
IN_DATE/MDYY = CHKIN_DATE;
INV_DT/YYMD = INVOICE_DATE;
NBR_DAYS/D12 = NUM_DAYS;
NBR_ROOMS/D12 = NUM_ROOMS;
NEW_ROOM_RATE/D12.2CSM = ROOM_RATE;
NEW_TOTAL_AMT/D12.2CSM = TOTAL_AMT;

NNROOMS/D12 = IF ((NBR_DAYS LT 0) AND (NUM_ROOMS LT 0)) THEN 
                ((NBR_DAYS*NUM_ROOMS) * (-1)) ELSE NBR_DAYS*NUM_ROOMS;
OUT_DATE/MDYY = CHKOUT_DATE;
RATE_GRP/A1 = IF ((ROOM_RATE GE 150 AND ROOM_RATE LE 199) OR
                  (ROOM_RATE LE (-150) AND ROOM_RATE GE (-199)))
                  THEN 'Y' ELSE 'N';
REFUSE_CD/A2=EDIT(REFUSE_KEY,'$$$$$$$$99');

END
-RUN

TABLEF FILE &&EXTRACT
PRINT APP_FLAG
  CITY_ST  
  CHAIN_NAME
  HTL_KEY     
  IN_DATE 	
  INV_DT      
  INVOICE_DATE 	
  LEVEL3 	
  NBR_DAYS    
  NBR_ROOMS
  NEW_ROOM_RATE
  NEW_TOTAL_AMT       
  NNROOMS	
  OUT_DATE    
  PASSNGR_NAME 	
  PROP_NAME 
  REFUSE_CD
  REFUSE_KEY
      
