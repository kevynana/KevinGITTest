-* Select only Exchange Sale records for the time period and excluding MCOs
DEFINE FILE SR_50
-INCLUDE TRANTYPE
END
-RUN
TABLE FILE SR_50
PRINT PASSNGR_NAME AS 'PAXNAMA'
PNR_LOCATOR AS 'PNRA'
TKT_DATE AS 'DATEA'
C_ITIN AS 'ITINA'
REFUSE_CODE AS 'CODEA'
TKT_NUM AS 'TICKETA'
DOC_NUMBER AS 'TICKET1'
EXCHG_SALE AS 'EXSALEA'
VAL_AIRLINE AS 'VALAIRA'
BY DOC_NUM
WHERE EXCHG_SALE NE ' '
-INCLUDE &AIRDATES
WHERE VOID_DATE EQ 0
WHERE DOC_TYPE NE '1'
-INCLUDE RPTPARMS
ON TABLE HOLD AS EHOLD1
END
-RUN


