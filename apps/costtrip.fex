-INCLUDE SETECHO

-INCLUDE DTRANGE
-RUN

-SET &&HEADING = 'COST PER TRIP';
-RUN

SET ASNAMES=ON

-SET &&ES5_MAIN = 
- &&WEB_PATH || 'ES5_MAIN.FOC';
-SET &&ES1_MAIN = 
- &&WEB_PATH || 'ES1_MAIN.FOC';
-RUN
FILEDEF ES5_MAIN DISK &&ES5_MAIN
FILEDEF ES1_MAIN DISK &&ES1_MAIN
-RUN

-SET &&RPTSUF = 'COSTTRIP';
-INCLUDE FDEFRPTS
-RUN

CREATE FILE ES5_MAIN
END
-RUN

-* TOTAL AIRLINE TICKETS
-* TOTAL

DEFINE FILE ES1_MAIN
ROW_LABELX/A21 = 'Total Airline Tickets';
ROW_TYPE/A13 = '             ';
END
-RUN
TABLE FILE ES1_MAIN
PRINT CATEGORY ROW_LABELX AS 'ROW_LABEL' ROW_TYPE CAT_ORDER ROW_ORDER
DIYTD_PRIOR AS 'PERIOD1' DIYTD_CURR AS 'PERIOD2' DIYTD_VAR_PCT AS 'PERCENT12'
DI_PRIOR AS 'PERIOD3' DI_CURR AS 'PERIOD4' DI_VAR_PCT AS 'PERCENT34'
IF ROW_LABEL EQ 'Net Tickets';
ON TABLE HOLD
END
-RUN

MODIFY FILE ES5_MAIN
FIXFORM FROM HOLD
-* FIXFORM CATEGORY/30 ROW_LABEL/21 ROW_TYPE/13 CAT_ORDER/2 ROW_ORDER/2
-* FIXFORM PERIOD1/10 PERIOD2/10
-* FIXFORM PERCENT12/4 PERIOD3/10 PERIOD4/10 PERCENT34/4
MATCH CATEGORY ROW_LABEL ROW_TYPE
ON NOMATCH COMPUTE ROW_ORDER=01;
ON NOMATCH COMPUTE CATEGORY='AIR';
ON MATCH REJECT
ON NOMATCH INCLUDE
DATA ON HOLD
END
-RUN

