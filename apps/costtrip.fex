ROW_LABEL/A21 = 'AVERAGE COST PER TRIP';
ROW_TYPE/A13 = '             ';
CAT_ORDER/I2 = 05;
ROW_ORDER/I2 = 04;
PERIOD1/D10 = IF APERIOD1 = 0 THEN 0 ELSE BPERIOD1/APERIOD1;
PERIOD2/D10 = IF APERIOD2 = 0 THEN 0 ELSE BPERIOD2/APERIOD2;
PERIOD3/D10 = IF APERIOD3 = 0 THEN 0 ELSE BPERIOD3/APERIOD3;
PERIOD4/D10 = IF APERIOD4 = 0 THEN 0 ELSE BPERIOD4/APERIOD4;
PERCENT12/I4 = IF PERIOD1 = 0 THEN 0 ELSE (PERIOD2 - PERIOD1)/PERIOD1*100;
PERCENT34/I4 = IF PERIOD3 = 0 THEN 0 ELSE (PERIOD4 - PERIOD3)/PERIOD3*100;
END
-RUN

