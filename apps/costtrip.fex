DEFINE FILE ES5_MAIN
   VARIANCE34/D10C = PERIOD4 - PERIOD3;
   PERCENT34/P4 = IF ((PERIOD3 LT 1) AND
			(PERIOD3 GT (-1))) THEN 0 ELSE
                        ((VARIANCE34 / PERIOD3) * 100) ;

   VARIANCE12/D10C = PERIOD2 - PERIOD1;
   PERCENT12/P4 = IF ((PERIOD1 LT 1) AND
			(PERIOD1 GT (-1))) THEN 0 ELSE
                        ((VARIANCE12 / PERIOD1) * 100) ;

   ROW_LABELA/A21 = IF (ROW_LABEL EQ 'AVERAGE COST PER TRIP') AND
   (ROW_ORDER EQ 05 OR ROW_ORDER EQ 06) THEN ' ' ELSE ROW_LABEL ;
   NOWTOD/A8 WITH CAT_ORDER = HHMMSS (NOWTOD);
END
-RUN

