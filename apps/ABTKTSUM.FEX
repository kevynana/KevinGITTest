DEFINE FILE XTHREE
 
NOWTOD/A8 WITH TKT_NUM = HHMMSS (NOWTOD);
AFLAGA/A1 = IF AFLAG EQ 'A' THEN 'Y' ELSE 'N';
HFLAGA/A1 = IF HFLAG1 EQ 'H' OR HFLAG2 EQ 'H' OR HFLAG3 EQ 'H' OR HFLAG4 EQ 'H' OR HFLAG5 EQ 'H' THEN 'Y' ELSE 'N';
CFLAGA/A1 = IF CFLAG1 EQ 'C' OR CFLAG2 EQ 'C' OR CFLAG3 EQ 'C' OR CFLAG4 EQ 'C' OR CFLAG5 EQ 'C' THEN 'Y' ELSE 'N';
LFLAGA/A1 = IF LFLAG1 EQ 'L' OR LFLAG2 EQ 'L' OR LFLAG3 EQ 'L' OR LFLAG4 EQ 'L' OR LFLAG5 EQ 'L' THEN 'Y' ELSE 'N';

-* ZFLAG/A1 = IF FARE_PAID EQ 0 AND H1_TAMT EQ 0 AND RENTAL_DAYS EQ 0 AND
-*            H1_TAMT EQ 0 AND H1NNROOMS EQ 0 THEN 'Y' ELSE 'N';
-*             AND LRAMT EQ 0 THEN 'Y' ELSE 'N';
ZFLAG/A1= IF FARE_PAID EQ 0 AND H1_TAMT AND H1NNROOMS EQ 0 AND C1_CRENTALA AND C1_CRENTAL_DAYS EQ 0 AND L1_LRENTALA AND L1_LRENTAL_DAYS EQ 0 THEN 'Y' ELSE 'N';
-* TT/A5 = IF AFLAG EQ 'A' AND HFLAG1 EQ ' ' AND HFLAG2 EQ ' ' AND HFLAG3 EQ ' ' AND CFLAG1 EQ ' ' AND CFLAG2 EQ ' ' AND CFLAG3 EQ ' ' THEN 'A' ELSE
-*         IF (AFLAG EQ 'A') AND (HFLAG1 EQ 'H' OR HFLAG2 EQ 'H' OR HFLAG3  EQ 'H') AND (CFLAG1 EQ ' ' AND CFLAG2 EQ ' ' AND CFLAG3 EQ ' ' THEN 'AH' ELSE
-*         IF AFLAG EQ 'A' AND HFLAG2 EQ 'H' AND CFLAG1 EQ ' ' AND CFLAG2 EQ ' ' AND CFLAG3 EQ ' ' THEN 'AH' ELSE
-*         IF AFLAG EQ 'A' AND HFLAG3 EQ 'H' AND CFLAG1 EQ ' ' AND CFLAG2 EQ ' ' AND CFLAG3 EQ ' ' THEN 'AH' ELSE 
-*         IF AFLAG EQ ' ' AND HFLAG1 EQ 'H' AND CFLAG1 EQ ' ' AND CFLAG2 EQ ' ' AND CFLAG3 EQ ' ' THEN 'H' ELSE
-*         IF AFLAG EQ ' ' AND HFLAG2 EQ 'H' AND CFLAG1 EQ ' ' AND CFLAG2 EQ ' ' AND CFLAG3 EQ ' ' THEN 'H' ELSE
-*        IF AFLAG EQ ' ' AND HFLAG3 EQ 'H' THEN 'H' ELSE ' ';
TT/A5 = IF AFLAGA EQ 'Y' AND HFLAGA EQ 'N' AND CFLAGA EQ 'N' AND LFLAGA EQ 'N' THEN 'A' ELSE
        IF AFLAGA EQ 'Y' AND HFLAGA EQ 'Y' AND CFLAGA EQ 'N' AND LFLAGA EQ 'N' THEN 'AH' ELSE
        IF AFLAGA EQ 'Y' AND HFLAGA EQ 'N' AND CFLAGA EQ 'Y' AND LFLAGA EQ 'N' THEN 'AC' ELSE
        IF AFLAGA EQ 'Y' AND HFLAGA EQ 'Y' AND CFLAGA EQ 'Y' AND LFLAGA EQ 'N' THEN 'ACH' ELSE
        IF AFLAGA EQ 'A' AND HFLAGA EQ 'N' AND CFLAGA EQ 'N' AND LFLAGA EQ 'Y' THEN 'AL' ELSE
        IF AFLAGA EQ 'N' AND HFLAGA EQ 'Y' AND CFLAGA EQ 'N' AND LFLAGA EQ 'N' THEN 'H' ELSE
        IF AFLAGA EQ 'N' AND HFLAGA EQ 'Y' AND CFLAGA EQ 'Y' AND LFLAGA EQ 'N' THEN 'CH' ELSE
        IF AFLAGA EQ 'N' AND HFLAGA EQ 'N' AND CFLAGA EQ 'Y' AND LFLAGA EQ 'N' THEN 'C' ELSE
        IF AFLAGA EQ 'N' AND HFLAGA EQ 'N' AND CFLAGA EQ 'N' AND LFLAGA EQ 'Y' THEN 'L' ELSE ' ';
        
 -* -INCLUDE DefineSubtotalFields  
END
