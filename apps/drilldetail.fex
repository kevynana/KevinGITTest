-* File drilldetail.fex

JOIN AIR_KEY IN A8JOINT TO ALL AIR_KEY IN S8JOINT AS D1
END

DEFINE FILE A8JOINT
FARE_PAID/D12.2 = SEG_AMT + SEG_TAX;
END

TABLE FILE A8JOINT
PRINT PAXNAM TKTNBR SEGNBR EMBARK ROUTE FARE_PAID
BY PAXNAM NOPRINT
BY TKTNBR NOPRINT
BY SEGNBR NOPRINT
WHERE AROLL_LEV1='JOINT-R'
ON TABLE SET ONLINE-FMT PDF
END
