TABLE FILE HOLD4
-INCLUDE HEADER
SUM TICK_SEG NOPRINT
COMPUTE TOT_SEG/I9C=TICK_SEG+ATICK_SEG; AS 'TOTAL MARKETS'
TICK_SEG AS 'ALN,MARKETS'
COMPUTE CRR_SHR/D6.1= IF ((TOT_SEG LT 1) AND (TOT_SEG GT (-1))) THEN 0 ELSE ((TICK_SEG/TOT_SEG)*100); AS 'ALN % OF,MARKETS'
FARE/D12.2C AS 'ALN,BASE,FARE'
COMPUTE TOT_REV/D12.2C=FARE+AFARE; AS 'TOTAL,BASE FARE'
BY RANK_VALUE AS 'RANK'
BY RANK_LABEL NOPRINT
BY EMB_LABEL AS 'MARKET'
BY RTE_LABEL AS ' '
ON RANK_LABEL RECOMPUTE MULTILINES AS 'TOTAL FOR'
 
 
