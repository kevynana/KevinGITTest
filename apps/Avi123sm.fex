-SET &&S_SUBJ8='COMPUTE COST_PER_KILO/P12.2C= IF ((KILO LT 1) AND';
-SET &&S_SUBJ9='(KILO GT (-1))) THEN 0 ELSE (GFARE/KILO); AS';
-SET &&S_SUBJ10=' ''COST,/KLMTR'' GVSAV/D16.2 AS ' | '''&GCCS_CUR.EVAL''';
-SET &&SUMM_ON4='ON ORG_PRINT RECOMPUTE NET_TKT_CNT';
-SET &&SUMM_ON5='GFARE KILO AVG_FLT COST_PER_KILO GVSAV AS ''TOTAL''';

