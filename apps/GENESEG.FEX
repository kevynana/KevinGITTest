

FILEDEF GENESEG DISK D:\TNT\TTRACKER\SYSTEM\DATA\GENESEG.TXT

SET ASNAMES=ON


DEFINE FILE GENESEG
HTLD/I5 = HTL_DAYS;
CARD/I5 = CAR_DAYS;
END

TABLE FILE GENESEG
SUM HTLD AS 'HTL_DAYS' CARD AS 'CAR_DAYS'
BY AIR_KEY
BY EMBARK
BY ROUTE
BY TKT_SORT
BY SEG_NBR
WHERE TKT_SORT CONTAINS '7143455063'
-* ON TABLE HOLD
ON TABLE COLUMN-TOTAL
END
-RUN
-QUIT


MODIFY FILE S3GENE
FIXFORM FROM HOLD
MATCH AIR_KEY EMBARK ROUTE TKT_SORT SEG_NBR
ON MATCH UPDATE HTL_DAYS CAR_DAYS
DATA ON HOLD
END
-RUN







