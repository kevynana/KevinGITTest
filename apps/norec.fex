-WRITE NOREC Report Name - &&RN
-WRITE NOREC Set key - &&SKEY
-WRITE NOREC Instance Key - &&IKEY
-IF &&RPTTYP EQ 'STS' GOTO TS_DATES;
-IF &&RPTTYP EQ 'SPD' GOTO DUAL_DATES;
-WRITE NOREC Date Range - FROM = &&FROMDT  TO = &&TODT
