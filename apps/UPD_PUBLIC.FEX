-INCLUDE SETECHO

SET ASNAMES = ON;



TABLE FILE A_PUBLIC
PRINT AIR_KEY
BY AIR_KEY NOPRINT
IF BR_CL_IDX EQ 'SE560920'
ON TABLE HOLD
END
-RUN

MODIFY FILE A_PUBLIC
FIXFORM FROM HOLD
MATCH AIR_KEY
ON MATCH COMPUTE LEVEL1='PUBLICG';
ON MATCH UPDATE LEVEL1
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN


TABLE FILE S_PUBLIC
