SET ASNAMES=ON
TABLE FILE ROLLSET
BY USE_PARM NOPRINT
BY GLOBAL_PARM NOPRINT
PRINT USE_PARM    AS 'VIEW_NAME
      RPT_GROUP   AS 'GROUP'
      GLOBAL_PARM AS 'SET_FILE'
      RPT_STREAM  AS 'STREAM_NAME'
      ROLLUP_CODE AS 'ROLLUP_CODE'
ON TABLE HOLD
END
MODIFY FILE VIEW
FIXFORM FROM HOLD
MATCH VIEW_NAME 
ON NOMATCH INCLUDE
MATCH SET_FILE
ON NOMATCH INCLUDE
ON MATCH UPDATE *
DATA ON HOLD
END
