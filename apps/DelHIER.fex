USE F:\TNT\TTRACKER\SYSTEM\DATA\HIERORG.FOC
END

TABLE FILE HIERORG
PRINT ROLLUP_CODE LEVEL ROLL_LEV DEPTNAME
BY ROLLUP_CODE NOPRINT
BY LEVEL NOPRINT
BY ROLL_LEV NOPRINT
BY DEPTNAME NOPRINT
WHERE ROLLUP_CODE EQ 'KIMB-R';
ON TABLE HOLD
END

MODIFY FILE HIERORG
FIXFORM FROM HOLD
MATCH ROLLUP_CODE LEVEL ROLL_LEV DEPTNAME
ON MATCH DELETE
ON NOMATCH REJECT
DATA ON HOLD
END
