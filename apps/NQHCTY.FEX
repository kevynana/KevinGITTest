-************************************************************************************
-* Get Values by Quarter for Base and Peer
-************************************************************************************
DEFINE FILE QRHBCTY
BNROOMS/D12 = IF ROLL EQ 'Base Company' THEN NNROOMS ELSE 0;
BNEW_TTL/D12 = IF ROLL EQ 'Base Company' THEN NEW_TOTAL_AMT ELSE 0;
PNROOMS/D12 = IF ROLL NE 'Base Company' THEN NNROOMS ELSE 0;
PNEW_TTL/D12 = IF ROLL NE 'Base Company' THEN NEW_TOTAL_AMT ELSE 0;
END
-RUN

TABLE FILE QRHBCTY
SUM BNROOMS BNEW_TTL PNROOMS PNEW_TTL NNROOMS
COMPUTE AVGBQ/D12 = IF ((BNROOMS LT 1) AND (BNROOMS GT (-1))) THEN 0 ELSE
                         (BNEW_TTL/BNROOMS);
COMPUTE AVGPQ/D12 = IF ((PNROOMS LT 1) AND (PNROOMS GT (-1))) THEN 0 ELSE
                         (PNEW_TTL/PNROOMS);
BY YEAR
BY TRAN
ON TABLE HOLD AS HOLD1
END
-RUN

TABLE FILE QRHBCTY
SUM BNROOMS AS 'BNROOMY' BNEW_TTL AS 'BNEW_TTLY' PNROOMS PNEW_TTL 
COMPUTE AVGBY/D12 = IF ((BNROOMS LT 1) AND (BNROOMS GT (-1))) THEN 0 ELSE
                          (BNEW_TTL/BNROOMS);
COMPUTE AVGPY/D12 = IF ((PNROOMS LT 1) AND (PNROOMS GT (-1))) THEN 0 ELSE
                         (PNEW_TTL/PNROOMS);
BY YEAR
ON TABLE HOLD AS THLD1
END
-RUN

DEFINE FILE THLD1
YR1/A2 = EDIT (YEAR, '$$99');
YR/A7 =  'YTD'| YR1;
END
-RUN

TABLE FILE THLD1
SUM BNROOMY BNEW_TTLY AVGBY AVGPY YR AS 'TRAN' 
BY YEAR
ON TABLE HOLD AS THLD2
END
-RUN


TABLE FILE THLD2
SUM BNROOMY BNEW_TTLY AVGBY AVGPY TRAN 
BY YEAR
BY TRAN
ON TABLE HOLD AS THLD3
END
-RUN

MATCH FILE HOLD1
PRINT BNROOMS BNEW_TTL PNROOMS PNEW_TTL AVGBQ AVGPQ 
BY YEAR
BY TRAN
ON TABLE HOLD
RUN
FILE THLD3
SUM BNROOMY BNEW_TTLY AVGBY AVGPY TRAN
BY YEAR
BY TRAN
AFTER MATCH HOLD AS HOLD4 OLD-OR-NEW
END
-RUN

DEFINE FILE HOLD4
TRN/A1 = EDIT(TRAN, '9$$$$$$');
BNROOMS1/D12 = IF TRN EQ 'Y' THEN BNROOMY ELSE BNROOMS;
BNEW_TTL1/D12 = IF TRN EQ 'Y' THEN BNEW_TTLY ELSE BNEW_TTL;
AVGBASE/D12 = IF TRN EQ 'Y' THEN AVGBY ELSE AVGBQ;
AVGPEER/D12 = IF TRN EQ 'Y' THEN AVGPY ELSE AVGPQ;
END
-RUN

TABLE FILE HOLD4
PRINT BNROOMS1 BNEW_TTL1 AVGBASE AVGPEER
BY YEAR NOPRINT
BY TRAN
ON TABLE SAVE AS HSQTR
END
-RUN


