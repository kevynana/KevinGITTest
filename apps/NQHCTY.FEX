-* Start building Report from file created
-***************************************************
TABLE FILE QRHCTY
SUM BOOKINGS HNNROOMS HTL_CNT NEW_TOTAL_AMT NNROOMS 
COMPUTE AVGRR/D12 = IF ((NNROOMS LT 1) AND (NNROOMS GT (-1))) THEN 0 ELSE
                       NEW_TOTAL_AMT/NNROOMS;
COMPUTE AVGS/D12 =  IF ((BOOKINGS LT 1) AND (BOOKINGS GT (-1))) THEN 0 ELSE
                       NNROOMS/BOOKINGS;
COMPUTE T3PCT/D12 = IF ((NNROOMS LT 1) AND (NNROOMS GT (-1))) THEN 0 ELSE
                       ((HNNROOMS/NNROOMS)*100);
BY CTY_VALUE
BY CTY_PRINT
ON TABLE HOLD AS BHOLD1
END
-RUN

TABLE FILE BHOLD1
PRINT BOOKINGS HNNROOMS HTL_CNT NEW_TOTAL_AMT NNROOMS CTY_VALUE 
      AVGRR AVGS T3PCT
BY CTY_PRINT
ON TABLE HOLD AS BHOLD2
END
-RUN


DEFINE FILE QRHPEER
ROLL1/A12 = 'Peer Group';
END
-RUN
TABLE FILE QRHPEER
SUM NNROOMS NEW_TOTAL_AMT
BY ROLL1
BY CTY_PRINT
ON TABLE HOLD AS PGHHLD
END
-RUN


