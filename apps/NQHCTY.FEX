DEFINE FILE HTLB1
CTTLAMT/D12 = IF (INVOICE_DATE GE '&&FROMDT.EVAL') AND 
                 (INVOICE_DATE LE '&&TODT.EVAL') THEN NEW_TOTAL_AMT ELSE 0;
CROOMS/D12 = IF (INVOICE_DATE GE '&&FROMDT.EVAL') AND 
                (INVOICE_DATE LE '&&TODT.EVAL') THEN NNROOMS ELSE 0;
PTTLAMT/D12 = IF (INVOICE_DATE GE '&&FXDATE.EVAL') AND 
                 (INVOICE_DATE LE '&&PYTDDATE.EVAL') THEN NEW_TOTAL_AMT ELSE 0;
PROOMS/D12 = IF (INVOICE_DATE GE '&&FXDATE.EVAL') AND 
                (INVOICE_DATE LE '&&PYTDDATE.EVAL') THEN NNROOMS ELSE 0;
FLAG/A1 = 'A';
END
-RUN

TABLE FILE HTLB1
SUM CTTLAMT CROOMS PTTLAMT PROOMS
COMPUTE CAVGR/D12 = IF ((CROOMS LT 1) AND (CROOMS GT (-1))) THEN 0 ELSE
                       (CTTLAMT/CROOMS);
COMPUTE PAVGR/D12 = IF ((PROOMS LT 1) AND (PROOMS GT (-1))) THEN 0 ELSE
                       (PTTLAMT/PROOMS);
BY FLAG
ON TABLE HOLD AS HTLCM1
END
-RUN

TABLE FILE HTLCM1
SUM CTTLAMT PTTLAMT
COMPUTE HSPEND/D4 = IF ((PTTLAMT LT 1) AND (PTTLAMT GT (-1))) THEN 0 ELSE
                       (CTTLAMT - PTTLAMT)/PTTLAMT*100;
COMPUTE HNIGHT/D4 = IF ((PROOMS LT 1) AND (PROOMS GT (-1))) THEN 0 ELSE
                       (CROOMS - PROOMS)/PROOMS*100;
COMPUTE HAVG/D4 = IF ((PAVGR LT 1) AND (PAVGR GT (-1))) THEN 0 ELSE
                     (CAVGR - PAVGR)/PAVGR*100;
COMPUTE ABSHS/D4 = ABS(HSPEND);
COMPUTE ABSHN/D4 = ABS(HNIGHT);
COMPUTE ABSHA/D4 = ABS(HAVG);
  
COMPUTE HSINC/A12 = IF HSPEND GT 0 THEN 'increased' ELSE IF ABSHS GT -1 THEN 'decreased' ELSE 'remains same';
COMPUTE HNINC/A12 = IF HNIGHT GT 0 THEN 'increased' ELSE IF ABSHN GT -1 THEN 'decreased' ELSE 'remains same'; 
COMPUTE HAINC/A12 = IF HAVG   GT 0 THEN 'increased' ELSE IF ABSHA GT -1 THEN 'decreased' ELSE 'remains same'; 
BY FLAG
ON TABLE HOLD AS HTLCM2
END
-RUN

TABLE FILE HTLCM2
PRINT HSPEND HNIGHT HAVG ABSHS ABSHN ABSHA HSINC HNINC HAINC
BY FLAG
ON TABLE SAVE AS HTLCMMT
END
-RUN

-SET &FLAG = ' ';
-SET &HSPEND=' ';
-SET &HNIGHT=' ';
-SET &HAVG=' ';
-SET &HSINC = ' ';
-SET &HNINC = ' ';
