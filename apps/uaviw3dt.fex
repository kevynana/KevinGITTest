TABLE FILE &&RPT_HOLD
SUM DISSAVINGS AS 'DTSAVE'	
  FARE_PAID AS 'TFARE'	
  NET_TKT_CNT AS 'TTKT'	
  SEG_LOWEST AS 'TLOW'
BY LEVEL_DESC
BY XPSNGR_NM
BY TKT_SORT
BY TRN_DATE
ON TABLE HOLD AS AIRHOLD1
END
-RUN

TABLE FILE &&RPT_HOLD
PRINT ADV_CAT
  ADV_DAYS
  AIRLINE
  AIR_KEY
  DPT_TIME
  EMB_CTY_NM
  EMB_APT_CD
  NET_TKT_CNT
  NEW_ADV_PUR
  REFUSE_CODE
  RTE_CTY_NM
  TKT_NUM
  TKT_TYPE
  XDPT_DT
  XTRAN_DT 
BY LEVEL_DESC
BY XPSNGR_NM
BY TKT_SORT
BY TRN_DATE
BY SEG_NBR
ON TABLE HOLD AS XTWO
END
-RUN

MATCH FILE XTWO
PRINT ADV_CAT
  ADV_DAYS
  AIRLINE
  AIR_KEY
  DPT_TIME
  EMB_APT_CD
  EMB_CTY_NM
  NET_TKT_CNT
  NEW_ADV_PUR
  REFUSE_CODE
  RTE_CTY_NM
  TKT_NUM
  TKT_TYPE
  XDPT_DT 
  XTRAN_DT
BY LEVEL_DESC
BY XPSNGR_NM
BY TKT_SORT
BY TRN_DATE
BY SEG_NBR
ON TABLE HOLD
RUN
FILE AIRHOLD1
PRINT DTSAVE	
  TFARE	
  TTKT	
  TLOW
BY LEVEL_DESC
BY XPSNGR_NM
BY TKT_SORT
BY TRN_DATE
AFTER MATCH HOLD AS AIRHOLD2 OLD-OR-NEW
END
-RUN

TABLE FILE AIRHOLD2
PRINT ADV_CAT
  ADV_DAYS
  AIRLINE
  DPT_TIME
  EMB_APT_CD
  EMB_CTY_NM  

  LEVEL_DESC
