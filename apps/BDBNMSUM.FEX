TABLE FILE XTWO
SUM EMP_ID AS 'Associate ID' PASSNGR_NAME AS 'Employee,Name '
 CAVOL/D12.2CS AS 'Total Air,Volume'
CNTKT AS 'Net,Tickets'
CLS_BD/D12.2CS AS '  Total,Lost Savings, (excludes CX)'
COMPUTE BDLSPCT/P12S% = IF ((BD_CAVOL LT 1) AND (BD_CAVOL GT (-1))) THEN 0 ELSE (CLS_BD/BD_CAVOL)*100; AS
                          '% of Total,Lost Savings to, Total Air Volume ,(excludes CX)'
 
CDATKT AS 'Domestic,Average,Ticket,Price'
CADVP1/D12%S AS '% of,Net Tickets Purchased,0-6 Days'
CADVP2/D12%S AS '% of,Net Tickets Purchased,7-13 Days'
 
-*CDONL AS '# of, Domestic,Online Bookings'
CDONLP/D12%S AS '% of Domestic,Online Bookings'
-*AVGCAR AS 'Average Car, Rate'
COMPUTE AVGCAR/D12.2CS = IF ((CRENTAL LT 1) AND (CRENTAL GT (-1))) THEN 0
                         ELSE (CRENTALA/CRENTAL);AS 'Average Car, Rate'
CRENTAL/D12CS AS 'Number of, Car Days' 
-*AVGRATE AS 'Average,Hotel,Cost Per,Night'
COMPUTE AVGRATE/D12.2CS = IF ((CHTTLN LT 1) AND (CHTTLN GT (-1))) THEN 0 ELSE
                             (CHTTLA/CHTTLN); AS 'Average,Hotel,Cost Per,Night'
CHTTLN/D12CS AS 'Number of,Hotel Nights'

COMPUTE TOTACH/D12.2CS = CAVOL + CRENTALA + CHTTLA; AS 'Total Air, Hotel and,Car Volume'
BY EMP_ID NOPRINT
BY PASSNGR_NAME NOPRINT



