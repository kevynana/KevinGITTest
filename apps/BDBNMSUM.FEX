-* -IF '&&ROLLUP.EVAL' NE 'BAIRD-R' GOTO STDCDESC;
-* CDESC/A5 = 'Baird';
-* -GOTO donebdchk;
-* -STDCDESC
CDESC/A6 = EDIT('&COMP', '99999');
-* -donebdchk;
CDATKT1/D12.2CSM = CDATKT;
