DEFINE FILE XTWO
NOWTOD/A8 WITH CAVOL = HHMMSS (NOWTOD);
BLANK/A1 = ' ';
PAX/A33 = LCWORD(33, PASSNGR_NAME, PAX);
CMF1/YYMD = &&FROMDT.EVAL;
CMF/A8MDYY = CMF1;
CMFM/A2 = EDIT(CMF, '99');
CMFD/A2 = EDIT(CMF, '$$99');
CMFY/A4 = EDIT(CMF, '$$$$9999');
CMFMC/A10  = IF CMFM EQ '01' THEN 'January' ELSE
            IF CMFM EQ '02' THEN 'February' ELSE
            IF CMFM EQ '03' THEN 'March' ELSE
            IF CMFM EQ '04' THEN 'April' ELSE
            IF CMFM EQ '05' THEN 'May' ELSE
            IF CMFM EQ '06' THEN 'June' ELSE
            IF CMFM EQ '07' THEN 'July' ELSE
            IF CMFM EQ '08' THEN 'August' ELSE
            IF CMFM EQ '09' THEN 'September' ELSE
            IF CMFM EQ '10' THEN 'October' ELSE
            IF CMFM EQ '11' THEN 'November' ELSE 'December';
DATE/A15 = CMFMC|' '|CMFY;
CAVOL1/D12.2CSM = CAVOL;
CLS_BD1/D12.2CSM = CLS_BD;
-* -IF '&&ROLLUP.EVAL' NE 'BAIRD-R' GOTO STDBDESC;
