-INCLUDE SETECHO



FILEDEF BAG_FEE DISK D:\TNT\TTRACKER\SYSTEM\DATA\BAG_FEE.FOC
FILEDEF AIRLINE_FEE DISK D:\TNT\TTRACKER\TEMP\AIRLINE_FEE.TXT

-RUN

CREATE FILE BAG_FEE
-RUN
 
 

TABLE FILE AIRLINE_FEE
PRINT  AIRLINE_NAME BAG1FEE BAG2FEE
BY AIRLINE
ON TABLE HOLD AS BAGHOLD
END
-RUN


MODIFY FILE BAG_FEE
FIXFORM FROM BAGHOLD
MATCH AIRLINE
ON MATCH REJECT
ON NOMATCH INCLUDE
DATA ON BAGHOLD
END
-RUN

TABLE FILE BAG_FEE
PRINT *
END
-RUN
-QUIT

