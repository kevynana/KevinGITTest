-* File UPDRPT.FEX

TABLE FILE ROLLSET
PRINT ROLLUP_CODE GLOBAL_PARM DESCRIP2 DESCRIP3
IF DESCRIP3 CONTAINS 'CLIENT'
ON TABLE HOLD
END


MODIFY FILE ROLLSET
FIXFORM FROM HOLD
MATCH ROLLUP_CODE GLOBAL_PARM
ON MATCH COMPUTE DESCRIP3=' ';
ON MATCH UPDATE DESCRIP3
ON NOMATCH REJECT
DATA ON HOLD
END




