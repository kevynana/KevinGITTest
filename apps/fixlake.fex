TABLE FILE D_LAKE
PRINT AIR_KEY EMBARK ROUTE TKT_SORT SEG_NBR SEG_AMT  
SEG_LOWEST SEG_CONTRACT SEG_DISCOUNT 
IF EMBARK EQ 'FP2' OR 'FP3' 
ON TABLE HOLD
END

MODIFY FILE D_LAKE
FIXFORM FROM HOLD
MATCH AIR_KEY EMBARK ROUTE TKT_SORT SEG_NBR
ON MATCH COMPUTE SEG_LOWEST=SEG_AMT;
ON MATCH COMPUTE SEG_CONTRACT=SEG_AMT;
ON MATCH COMPUTE SEG_DISCOUNT=SEG_AMT;
ON MATCH UPDATE SEG_LOWEST
ON MATCH UPDATE SEG_CONTRACT
ON MATCH UPDATE SEG_DISCOUNT
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN
