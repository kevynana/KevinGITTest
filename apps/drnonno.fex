DEFINE FILE &&EXTRACT
CITY_ST/A40 = PROP_STATE||(' '| ' '|PROP_CITY);
IN_DATE/MDYY = CHKIN_DATE;
NBR_DAYS/D12 = NUM_DAYS;
NBR_ROOMS/D12 = NUM_ROOMS;
NEW_ROOM_RATE/D12.2C = ROOM_RATE;
NEW_TOTAL_AMT/D12.2C = TOTAL_AMT;

NNROOMS/D12 = IF (NUM_DAYS LT 0) AND
                 (NUM_ROOMS LT 0) THEN
                 (NUM_DAYS * NUM_ROOMS) * (-1) ELSE
                 (NUM_DAYS * NUM_ROOMS);

OUT_DATE/MDYY = CHKOUT_DATE;
REFUSE_CD/A2=EDIT(REFUSE_KEY,'$$$$$$$$99');
-INCLUDE TRANTYPE
END
-RUN

TABLEF FILE &&EXTRACT
PRINT CITY_ST  
  IN_DATE 	
  NBR_DAYS    
  NBR_ROOMS
  NEW_ROOM_RATE
  NEW_TOTAL_AMT       
  NNROOMS	
  OUT_DATE    
  PASSNGR_NAME 	
  PROP_NAME 
  REFUSE_CD
-***************************************************************
-* Where statements to select data specified in user profile
-***************************************************************
&&WHERE1
&&WHERE2
&&WHERE3

-INCLUDE RPTPARMS
WHERE INVOICE_DATE GE &&FROMDT.EVAL AND INVOICE_DATE LE &&TODT.EVAL
&&NONCHK
&&LCHK
ON TABLE HOLD AS HHOLD
END
-RUN


-IF &RECORDS EQ 0 THEN GOTO NORECORDS;
