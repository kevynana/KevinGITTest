-GOTO XXIT;
 
-TP500;
TABLE FILE TCTPTVL
SUM NET_TKT_CNT
    FARE_PAID
    MILES
BY PASSNGR_NAME
BY ROLLCD
BY ROLL_NAME
ON TABLE HOLD AS THOLD1
END
-RUN
 
 
TABLE FILE THOLD1
PRINT *
-* BY ROLLCD
-* BY ROLL_NAME
-* BY PASSNGR_NAME
RANKED BY HIGHEST NET_TKT_CNT NOPRINT
ON TABLE HOLD AS TCTT3
END
-RUN
 
DEFINE FILE TCTT3
 RANK_LIMIT/I5 = 500;
 CRANK/D5 =   RANK;
 BRANK/A6 =   FTOA(CRANK, '(D5)' , 'A6');
 LIST/D6 = IF PASSNGR_NAME NE LAST PASSNGR_NAME THEN (LAST LIST + 1) ELSE LAST LIST;
 FRANKX/A6 = FTOA(LIST, '(D5)', 'A6');
 FRANK/A5=EDIT(FRANKX,'$99999');
 RANK_VALUE/A9 = IF (RANK_LIMIT EQ 0) THEN BRANK ELSE
                 IF LIST GT RANK_LIMIT THEN 'OTHER' ELSE FRANK;
 RANK_LABEL/A33 = IF RANK_VALUE NE 'OTHER' THEN PASSNGR_NAME ELSE 'OTHER';
 RANK_LABEL1/A9 = IF RANK_LABEL EQ 'OTHER' THEN 'ALL OTHER' ELSE ROLLCD;
 RANK_LABEL2/A25 = IF RANK_LABEL EQ 'OTHER' THEN ' ' ELSE ROLL_NAME;
END
-RUN
 
TABLE FILE TCTT3
SUM NET_TKT_CNT FARE_PAID MILES
BY RANK_VALUE
BY RANK_LABEL
BY RANK_LABEL1
BY RANK_LABEL2
ON TABLE HOLD AS TCHOLD1
END
-RUN
 
 
-SET &&RPTSUF = 'SMY';
-INCLUDE FDEFRPTS
-RUN
 
DEFINE FILE TCHOLD1
NOWTOD/A9 WITH RANK_LABEL1 = HHMMSS (NOWTOD);
END
