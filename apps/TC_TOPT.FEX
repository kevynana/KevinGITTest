EX TCAIRDB
-RUN
- IF &FOCERRNUM EQ 36 THEN GOTO IncLoop2;
 
-TYPE BEFORE EXTRACT
EX TCTTEX
-RUN
-IncLoop2
-SET &CNT = &CNT + 1;
-Loop2
 
-INCLUDE SetParmtime
-RUN
 
-IF &&FROMAVRP NE 'Y' GOTO :AVSF0;
-INCLUDE AVRP_SUBFOOT
-:AVSF0
 
-TYPE AFTER EXTRACT
 
-IF &&SETF EQ 'TCTPTVL1' GOTO TP500;
 
-******************************************************************
-* GET THE FINAL REPORT
-******************************************************************
DEFINE FILE TCTPTVL
RNK_LBL/A33 = IF RANK_VALUE EQ 'OTHER' THEN 'ALL OTHER' ELSE RANK_LABEL;
END
TABLE FILE TCTPTVL
SUM NET_TKT_CNT
    FARE_PAID
    MILES
BY ROLLCD
BY ROLL_NAME
BY RANK_VALUE
BY RANK_LABEL
BY RNK_LBL
ON TABLE HOLD AS THOLD1
END
-RUN


-SET &&RPTSUF = 'SMY';
-INCLUDE FDEFRPTS
-RUN
 
