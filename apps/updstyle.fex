
JOIN SET_KEY IN RPT_SET TO ALL SET_KEY IN RPT_INST
TABLE FILE RPT_SET
PRINT SET_NAME RPT_NAME SET_TYPE RPT_TYPE INST_KEY USE_PARM RPT_STREAM
BY ROLLUP_CODE
BY USER_NAME
WHERE RPT_STREAM EQ 'OV_MXOF'
ON TABLE HOLD AS JOYHOLD
END

TABLE FILE JOYHOLD
PRINT INST_KEY USE_PARM RPT_STREAM
BY INST_KEY
ON TABLE HOLD
END
MODIFY FILE RPT_INST
FIXFORM FROM HOLD
MATCH INST_KEY
ON MATCH DELETE
ON NOMATCH REJECT
DATA ON HOLD
END

-EXIT






