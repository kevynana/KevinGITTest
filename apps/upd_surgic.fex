SET ASNAMES = ON

-INCLUDE SETECHO


TABLE FILE UCSURGIC
PRINT CAR_KEY AUD_DATA AS 'LEVEL5'
BY CAR_KEY NOPRINT
IF UDID EQ TP-
ON TABLE HOLD
END
-RUN

MODIFY FILE C_SURGIC
FIXFORM FROM HOLD
MATCH CAR_KEY
ON MATCH UPDATE LEVEL5
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN

TABLE FILE UHSURGIC
PRINT HTL_KEY AUD_DATA AS 'LEVEL5'
BY HTL_KEY NOPRINT
IF UDID EQ TP-
ON TABLE HOLD
END
-RUN

MODIFY FILE H_SURGIC
FIXFORM FROM HOLD
MATCH HTL_KEY
ON MATCH UPDATE LEVEL5
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN

