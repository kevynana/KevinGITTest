SET EDAPATH=+D:\IBI\SRV43\WFS\EDATEMP\TS000707;

TABLE FILE BRPT_QUE
PRINT *
ON TABLE HOLD AS HOLD1
END
MODIFY FILE RPT_QUE
FIXFORM FROM HOLD1
MATCH INST_KEY
  ON NOMATCH INCLUDE
  ON MATCH REJECT
DATA ON HOLD1
END

TABLE FILE RPT_QUE
PRINT INST_KEY
WHERE EX_FLAG EQ 'E'
ON TABLE HOLD AS HOLD2
END
MODIFY FILE RPT_QUE
FIXFORM FROM HOLD2
MATCH INST_KEY
 ON NOMATCH REJECT
 ON MATCH COMPUTE EX_FLAG='Y';
 ON MATCH UPDATE EX_FLAG
DATA ON HOLD2
END