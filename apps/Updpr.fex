-* File UPDRPT.FEX

TABLE FILE ROLLUP
PRINT ROLLUP_CODE AND PR_STAMP
IF ROLLUP EQ 'AGRI-R' OR 'ASI-R' OR 'CETAC-R' OR 'CUCU-R' OR 'DTNC-R' OR
             'GORD-R' OR 'IHI-R' OR 'PACE-R' OR 'SERG-R' OR 'TANDT-R' OR
             'TRAN-R'
ON TABLE HOLD
END


MODIFY FILE ROLLUP
FIXFORM FROM HOLD
MATCH ROLLUP_CODE
ON MATCH COMPUTE PR_STAMP='R';
ON MATCH UPDATE PR_STAMP
ON NOMATCH REJECT
DATA ON HOLD
END




