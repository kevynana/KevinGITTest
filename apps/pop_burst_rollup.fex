-* File pop_burst_rollup.fex
-DEFAULT &SCHED_OWNER='$*';
 
DEFINE FILE BURST_GROUPS
SEL/A1='N';
END
TABLE FILE BURST_GROUPS
SUM FST.ROLLUP_CODE AS ROLL
    SEL
BY ROLLUP_CODE AS ROLL
WHERE OWNER EQ '&SCHED_OWNER';
ON TABLE SET ASNAMES ON
ON TABLE SET HOLDLIST PRINTONLY
ON TABLE PCHOLD FORMAT XML
END
