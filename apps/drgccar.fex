DEFINE FILE RPTHOLD1
-******************************************************************************
-*                         Global Currency Conversion Defines
-******************************************************************************
NAMEC1/A30 = CNAME;
NAMEC/A30 = IF COUNTRYCODE EQ 'USD' THEN ' ' ELSE NAMEC1;
  CURR1X/D20.6 = 1 / USDPU ;
 
-* DEFINES ON PREVIOUS DEFINES
  CARAMT/D20.6 = NEW_RENTAL_AMT * CURR1X;
 
-* DEFINES ON PREVIOUS DEFINES
 
  CARAMTX/D20.6 = CARAMT;
 
-* DEFINES ON PREVIOUSLY DEFINED DEFINES
 
  CARAMT1/D15.2CS = CARAMTX;
  NOWTOD/A8 WITH NBR_DAYS = HHMMSS (NOWTOD);
END
-RUN
-TYPE Before Report

-SET &&PAHDR  = IF '&&OUTFMT.EVAL' EQ 'PDF' OR 'PPT' OR 'COM' THEN 'HEADAIR' ELSE 'HDREXLS'; 
-SET &&PASTYD = IF '&&OUTFMT.EVAL' NE 'PDF' OR 'PPT' OR 'COM' THEN 'DTEXL' ELSE &&DTSTY;
-RUN

 
TABLE FILE RPTHOLD1
-INCLUDE &&PAHDR
"&&SUBHEAD"
-IF &&PAHDR EQ 'HDREXLS' GOTO NOSPC;
"</2"
-NOSPC;
-*
-*******************************************************************
-* Sum statement with fields passed from the user profile database
-*******************************************************************
-*
SUM
NBR_DAYS AS 'DAYS'
BOOKINGS AS 'BOOKINGS'
CARAMT1 AS 'RENTAL,AMOUNT'
COMPUTE AVGDR/P12.2CS = IF CARAMT1 LT 0 AND NBR_DAYS LT 0
 THEN ((CARAMT1/NBR_DAYS) * (-1))
 ELSE CARAMT1/NBR_DAYS; AS 'AVERAGE,DAILY,RATE'
 
-*
-******************************************************************
-* By statements (one per field)
-******************************************************************
-*
BY PASSNGR_NAME AS 'PASSENGER NAME'
BY VENDOR_NAME AS 'VENDOR'
BY CAR_TYPE AS 'CAR'
BY PICK_UP_DT AS 'PICKUP,DATE'
BY PICK_CTY_NAME AS 'PICKUP,CITY'
BY DROP_OFF_DT AS 'DROP,DATE'
BY DROP_CTY_NAME AS 'DROP,CITY'
WHERE GLOBAL.COUNTRYCODE EQ '&&CURRENCY'
-*
-******************************************************************
-* On Table statements for code appropriate to On Table commands
-******************************************************************
-*
ON TABLE COLUMN-TOTAL
-* ON PASSNGR_NAME SKIP-LINE
ON PASSNGR_NAME SUMMARIZE AS 'TOTAL FOR'
-*
-******************************************************************
-* Report specific footer(s)
-******************************************************************
-*
FOOTING BOTTOM
" "
"&&FOOTER"
"Post-trip Reporting &DATE AT <NOWTOD <90 &&FOOTR"
-* "&&CRIGHT"
 
-*
