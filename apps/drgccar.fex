DEFINE FILE CR_50
CURRENT/A1 = IF INVOICE_DATE GE &&FROMDT.EVAL AND INVOICE_DATE LE &&TODT.EVAL THEN 'Y' ELSE 'N';
 
BOOKINGS/D8 = IF DAILY_RATE GE 0 THEN 1 ELSE
                  IF DAILY_RATE LT 0 THEN (-1) ELSE 0;
DROP_OFF_DT/MDYY = DROP_DATE;
PICK_UP_DT/MDYY = PICKUP_DATE;
NEW_RENTAL_AMT/D15.2 =  RENTAL_AMT;
 
NBR_DAYS/P10CS = IF (NUM_DAYS LT 0) AND
                  (NUM_CAR LT 0) THEN
                  (NUM_DAYS * NUM_CAR) * (-1) ELSE
                  (NUM_DAYS * NUM_CAR);
CDAY/P10CS = IF CURRENT EQ 'Y' THEN NBR_DAYS ELSE 0;
 
INV_DT/YYMD = INVOICE_DATE;
VNDR/A12 = LCWORD(12, VENDOR_NAME, VNDR);
END
-RUN
 
