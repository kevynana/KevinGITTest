TABLE FILE &&RPT_HOLD
SUM COMM_SEG AS 'TCOMM'
    CP_FARE_PD AS 'FARE'
    MILES
    NET_COMM AS 'NCOMM'
    NET_TKT_CNT AS 'TICK_SEG'
    TOTAL_FARE AS 'TFARE'
BY AIRSELECT
BY AIRLINE_NAME
BY ORGDST
BY CP_EMB
BY CP_RTE
BY EMB_APT
BY RTE_APT
WHERE CP_AIRLINE EQ '&&AIR1'
ON TABLE HOLD AS CPHOLD1
END
