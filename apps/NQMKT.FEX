COMPUTE YINTSC/D8 = IF ((TCIFARE LT 1) AND (TCIFARE GT (-1))) THEN 0 ELSE
                       ((BCIFARE/TCIFARE)*100);
COMPUTE YINTTC/D8 = IF ((TCIMKT LT 1) AND (TCIMKT GT (-1))) THEN 0 ELSE
                       ((BCIMKT/TCIMKT)*100);
COMPUTE ABSYINTSC/D4 = ABS(YINTSC);
COMPUTE ABSYINTTC/D4 = ABS(YINTTC);
ON TABLE HOLD AS INTCTOT2
END
-RUN

TABLE FILE INTCTOT2
PRINT YINTSC YINTTC ABSYINTSC ABSYINTTC 
ON TABLE SAVE AS INTCCMT
END
-RUN

-SET &YINTSC='';
-SET &YINTTC='';
-SET &ABSYINTSC='';
-SET &ABSYINTTC='';

-READ INTCCMT NOCLOSE &YINTSC.8. &YINTTC.8. &ABSYINTSC.4. &ABSYINTTC.4. 

-* Prior Data
DEFINE FILE ORGFIX3I
FLAG/A1 = 'A';
END
TABLE FILE ORGFIX3I
SUM BPIMKT
    BPIFARE
BY FLAG
WHERE ORG_VALUE NE 'ALL OTHER'
ON TABLE HOLD AS INTP1
END
-RUN

DEFINE FILE ORGFIX3I
FLAG/A1 = 'B';
END
TABLE FILE ORGFIX3I
SUM BPIMKT AS 'TPIMKT'
    BPIFARE AS 'TPIFARE'
BY FLAG
ON TABLE HOLD AS INTPTOT1
END
-RUN

MATCH FILE INTP1
PRINT BPIMKT
      BPIFARE
BY FLAG
ON TABLE HOLD
RUN
FILE INTPTOT1
SUM TPIMKT
    TPIFARE
BY FLAG
AFTER MATCH HOLD AS INTPTOT2 OLD-OR-NEW
END
-RUN

TABLE FILE INTPTOT2
SUM 
COMPUTE YINTSP/D8 = IF ((TPIFARE LT 1) AND (TPIFARE GT (-1))) THEN 0 ELSE
                       ((BPIFARE/TPIFARE)*100);
COMPUTE YINTTP/D8 = IF ((TPIMKT LT 1) AND (TPIMKT GT (-1))) THEN 0 ELSE
                       ((BPIMKT/TPIMKT)*100);
COMPUTE ABSYINTSP/D4 = ABS(YINTSP);
COMPUTE ABSYINTTP/D4 = ABS(YINTTP);
ON TABLE HOLD AS INTPTOT3
END
-RUN

TABLE FILE INTPTOT3
PRINT YINTSP YINTTP ABSYINTSP ABSYINTTP 
ON TABLE SAVE AS INTPRIORC
END
-RUN

-SET &YINTSP='';
-SET &YINTTP='';
-SET &ABSYINTSP='';
-SET &ABSYINTTP='';

-READ INTPRIORC NOCLOSE &YINTSP.8. &YINTTP.8. &ABSYINTSP.4. &ABSYINTTP.4. 



