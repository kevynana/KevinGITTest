TABLE FILE ORGFIX3
SUM BCDMKT
    PCT.BCDMKT AS 'BPCTMKT'
    BCDFARE
COMPUTE BCDAVGF/D6 = IF ((BCDMKT LT 1) AND (BCDMKT GT (-1))) THEN 0 ELSE
                      (BCDFARE/BCDMKT);
COMPUTE BCDCPM/D4.2 = IF ((BCDMILES LT 1) AND (BCDMILES GT (-1))) THEN 0 ELSE
                      (BCDFARE/BCDMILES);
COMPUTE PCDAVGF/D6 = IF ((PCDMKT LT 1) AND (PCDMKT GT (-1))) THEN 0 ELSE
                       (PCDFARE/PCDMKT);
COMPUTE PCDCPM/D4.2 = IF ((PCDMILES LT 1) AND (PCDMILES GT (-1))) THEN 0 ELSE
                      (PCDFARE/PCDMILES);
   BPDMKT
   PCT.BPDMKT AS 'PPCTMKT'
   BPDFARE
COMPUTE BPDAVGF/D6 = IF ((BPDMKT LT 1) AND (BPDMKT GT (-1))) THEN 0 ELSE
                      (BPDFARE/BPDMKT);
COMPUTE BPDCPM/D4.2 = IF ((BPDMILES LT 1) AND (BPDMILES GT (-1))) THEN 0 ELSE
                      (BPDFARE/BPDMILES);
