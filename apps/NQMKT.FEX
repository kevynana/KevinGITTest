                       (PPDFARE/PPDMKT);
COMPUTE PPDCPM/D4.2 = IF ((PPDMILES LT 1) AND (PPDMILES GT (-1))) THEN 0 ELSE
                      (PPDFARE/PPDMILES);
BY ORG_VALUE
BY ORG_PRINT1
BY DST_PRINT1
ON TABLE HOLD AS CDOM1 FORMAT FOCUS
END
