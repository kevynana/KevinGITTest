&&WHERE4
&&WHERE5
&&WHERE6
&&WHERE7
&&WHERE8
&&WHERE9
&&WHERE10

-INCLUDE RPTPARMS


ON TABLE HOLD AS AUXHOLD
END
-RUN

-IF &RECORDS EQ 0 GOTO XXIT;

