DEFINE FILE DRHHLD1
NOWTOD/A8 WITH LEVEL_DESC = HHMMSS (NOWTOD);
END
TABLE FILE DRHHLD1
-INCLUDE HEADAIR
SUM
BOOKINGS AS 'BOOKINGS'
NNROOMS AS 'NUMBER,NIGHTS'
TOTAL_RATE AS 'TOTAL,AMOUNT'
COMPUTE AR/D12.2CSM = IF ((NNROOMS LT 1) AND (NNROOMS GT (-1))) THEN 0 ELSE TOTAL_RATE/NNROOMS; AS 'AVERAGE,NIGHTLY,RATE'
