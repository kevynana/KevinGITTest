TABLE FILE DRHHLD
SUM BOOKINGS
    NNROOMS
    TOTAL_RATE
BY LEVEL_DESC
BY PASSNGR_NAME
BY INV_DT
BY IN_DATE
BY OUT_DATE
BY PROP_NAME
BY PROP_ADDRESS
BY PROP_ZIP5
BY CITY_ST
