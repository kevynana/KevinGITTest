JOIN SET_KEY IN RPT_INST TO SET_KEY IN RPT_SET AS J1
END

TABLE FILE RPT_INST
PRINT INST_KEY
BY INST_KEY
BY USER_NAME
WHERE USER_NAME EQ 'joseeb'
-* ON TABLE HOLD AS PHOLD
END
-RUN
-QUIT

TABLE FILE JOYHOLD
PRINT INST_KEY 
BY INST_KEY NOPRINT
ON TABLE HOLD AS PHOLD
END
-RUN

MODIFY FILE RPT_INST
FIXFORM FROM PHOLD
MATCH INST_KEY
ON MATCH DELETE
ON NOMATCH REJECT
DATA ON PHOLD
END
-RUN

