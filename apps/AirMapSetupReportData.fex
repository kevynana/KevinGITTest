-* 07/27/15 - DEB - S-10299 COMMENTED OUT SET &ECHO=ALL


-* AirMapSetupReportData.fex
-*-SET &ECHO=ALL;
-DEFAULT &&COMP = 'ABCD';
-DEFAULT &&ROLLUP = 'ABCD-R';
-DEFAULT &&ROLLFY='01';
 
SET GRAPHSERVURL=''

SET ASNAME = ON
-SET &&WEB_PATH= TEMPPATH(50,'A50');
APP MAP WEBTEMP &&WEB_PATH
APP APPENDPATH WEBTEMP
 
TABLE FILE CONTROL
PRINT ROLLUP_CODE COMP USER_ID SECURITY
ON TABLE SAVE AS HLDCTRL
END
-RUN
 
-READ HLDCTRL &&ROLLUP.8 &&COMP.6. &&UNAME.20. &&SECURITY.1.
-TYPE  &&ROLLUP &&COMP &&ROLLFY

TABLE FILE ROLLUP
PRINT COMP ROLL_FY QTR_ENABLE
ROLDET_POS ROLDET_SIZ 
WHERE ROLLUP_CODE EQ '&&ROLLUP.EVAL'
ON TABLE SAVE
END
-RUN
-READ SAVE &&COMP.6. &&ROLLFY.2. &&QTR.1. &&TNTDTPOS.10. &&ROLDTSIZ.10.
-*-SET &&COMP = 'CONG';

-SET &&ROLLPIC = 'ABCD.GIF';

-SET &&AIR_PATH = '&&DATASRV.EVAL' || '\TNT\TTRACKER\DATA\';
-*-SET &&AIR_PATH = 'C:\ibi\apps\data\';

-IF '&&QTR.EVAL' EQ 'X' THEN GOTO QTRData ELSE GOTO NonQTRData;

-NonQTRData
-SET &&SEG_DB = &&AIR_PATH || 'S_' || &&COMP || '.FOC'; 
-SET &&AIR_DB = &&AIR_PATH || 'A_' || &&COMP || '.FOC'; 
-SET &&TKT_DB = &&AIR_PATH || 'T_' || &&COMP || '.FOC'; 

USE
	&&SEG_DB AS SR_50
	&&AIR_DB AS AIR_MAIN
	&&TKT_DB AS TKT_MAIN
END
-RUN

-GOTO DONEALLOC;

-QTRData
-SET &&SEG_DB = &&AIR_PATH || 'S1' || &&COMP || '.FOC'; 
-SET &&AIR_DB = &&AIR_PATH || 'A1' || &&COMP || '.FOC'; 
-SET &&TKT_DB = &&AIR_PATH || 'T1' || &&COMP || '.FOC'; 

USE
	&&SEG_DB AS SR_50
	&&AIR_DB AS AIR_MAIN
	&&TKT_DB AS TKT_MAIN
END
-RUN

-DONEALLOC
CREATE FILE TMPAUDID
CREATE FILE TMPCUDID
CREATE FILE TMPHUDID