SET ASNAMES = ON


DEFINE FILE S9UPTTL
FARE_PAID/D12.2CS = SEG_AMT + SEG_TAX;
FLAG/A1 = IF SEG_STANDARD NE SEG_DISCOUNT THEN 'Y' ELSE
          IF SEG_STANDARD NE SEG_LOWEST THEN 'Y' ELSE
          IF SEG_LOWEST NE SEG_DISCOUNT THEN 'Y' ELSE 'N';
          
END
TABLE FILE S9UPTTL
SUM AIR_KEY SEG_STANDARD SEG_LOWEST SEG_DISCOUNT FARE_PAID
BY FLAG
BY AIR_KEY NOPRINT
WHERE ROUTE EQ 'EX1' OR EMBARK EQ 'EX1'
ON TABLE SET ONLINE-FMT EXL2K
END
-RUN
