TABLE FILE RPT_INST
PRINT INST_KEY AND EXECUTE_FLAG AND RPT_SERVER
IF EXECUTE_FLAG NE ' ';
IF RPT_SERVER NE ' ';
ON TABLE HOLD
END
-EXIT

MODIFY FILE RPT_INST
FIXFORM INST_KEY/A72 EXECUTE_FLAG/A1 RPT_SERVER/A20
MATCH INST_KEY
ON MATCH COMPUTE EXECUTE_FLAG=' ';
ON MATCH COMPUTE RPT_SERVER=' ';
ON MATCH UPDATE EXECUTE_FLAG
ON MATCH UPDATE RPT_SERVER
ON NOMATCH REJECT
DATA ON HOLD
END
