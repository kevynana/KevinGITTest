SET ASNAMES = ON
-RUN


DEFINE FILE &&RPT_HOLD
  DEPT/A15=LEVEL2;
  LOCATION/A15=LEVEL1;
  DIVISION/A8=TRVL_AUTH;
  DPCTY1/A3 = IF SEG_NBR EQ '01' THEN EMBARK ELSE ' ';
  FARE_PAID/D12.2 = TICKET_AMT + TAX_AMT;
  EMPLOYEE/A10 = GETTOK (INT_CODE, 36, 1, '-', 10, EMPLOYEE);
  PROJECT/A10 = GETTOK (LEVEL3, 15, 1, '.', 10, PROJECT);
  PHASE/A10 = GETTOK (LEVEL3, 15, -1, '.', 10, PHASE);
  DDATE1/YYMD = DPT_DATE;
  RDATE1/YYMD = RESV_DATE;

-* DEFINES ON PREVIOUS DEFINES
 END
-RUN

TABLE FILE &&RPT_HOLD
PRINT 
    DDATE1
    DEPT
    DIVISION
    DPCTY1
    EMPLOYEE
    FARE_PAID
    INTL_DOM
    LOCATION
    PROJECT
    PHASE
    PASSNGR_NAME
    RDATE1
    TOT_TNT_FEE
BY TKT_NUM
BY SEG_COUNT
WHERE SEG_NBR EQ '01'
ON TABLE HOLD AS HOLD1
END
-RUN


DEFINE FILE HOLD1
NOWTOD/A8 WITH TKT_NUM = HHMMSS (NOWTOD);
END
TABLE FILE HOLD1
-INCLUDE HEADAIR
"&&SUBHEAD"
"</2"
-*
-**********************************************************************
-* Print statement with fields passed from the user profile database
-**********************************************************************
-*
