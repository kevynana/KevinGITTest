-INCLUDE SETECHO
 
SET ASNAMES = ON
 
EX CONCAT_ALL_DBS ROLL=ABCD-R,FID=SEG,CLEAR=Y
-RUN

TABLE FILE SR_50
PRINT CITYPAIR.EMBARK CITYPAIR.ROUTE  
BY AIR_KEY
WHERE TRN_DATE GE '2017/08/01' AND TRN_DATE LE '2017/08/05'
WHERE RECORDLIMIT EQ 10
END
-RUN


TABLE FILE SR_50
PRINT EMBARK ROUTE
BY AIR_KEY
WHERE TRN_DATE GE '2017/08/01' AND TRN_DATE LE '2017/08/05'
WHERE RECORDLIMIT EQ 20
END
-RUN

-QUIT
