DEFINE FILE MSYTD ADD
  AIRSELECT/A3 = IF ('&&AIR1' EQ AIR_LINE) THEN AIR_LINE ELSE '0';
  PERIOD1/A1 = IF ((TRN_DATE GE &&FROMDT.EVAL) AND (TRN_DATE LE &&TODT.EVAL)) 
             THEN 'Y' ELSE 'N';
  MTCOMM/D12.2S = IF PERIOD1 EQ 'Y' THEN COMM_SEG ELSE 0;
  MFARE/D12.2S = IF PERIOD1 EQ 'Y' THEN CP_FARE_PD ELSE 0;
  GMFARE/D16.2CS = IF PERIOD1 EQ 'Y' THEN PAID_FAREX ELSE 0;
  MMILES/D9C = IF PERIOD1 EQ 'Y' THEN MILES ELSE 0;
  MKILO/D9C = IF PERIOD1 EQ 'Y' THEN KILO ELSE 0;
  MNCOMM/D12.2S = IF PERIOD1 EQ 'Y' THEN NET_COMM ELSE 0;
  MTICK_SEG/D8CS = IF PERIOD1 EQ 'Y' THEN NET_TKT_CNT ELSE 0;
  MTFARE/D12.2S = IF PERIOD1 EQ 'Y' THEN TOTAL_FARE ELSE 0;
  GMTFARE/D16.2CS = IF PERIOD1 EQ 'Y' THEN FARE_TOTX ELSE 0;
  MACOMM/D12.2S = IF PERIOD1 EQ 'Y' THEN COMM_SEG ELSE 0;
  MAFARE/D12.2S = IF PERIOD1 EQ 'Y' THEN CP_FARE_PD ELSE 0;
  GMAFARE/D16.2CS = IF PERIOD1 EQ 'Y' THEN PAID_FAREX ELSE 0;
  MANET/D12.2S = IF PERIOD1 EQ 'Y' THEN NET_COMM ELSE 0;
  MATICK_SEG/D8CS = IF PERIOD1 EQ 'Y' THEN NET_TKT_CNT ELSE 0;
  MATFARE/D12.2S = IF PERIOD1 EQ 'Y' THEN TOTAL_FARE ELSE 0;
  GMATFARE/D16.2CS = IF PERIOD EQ 'Y' THEN FARE_TOTX ELSE 0;
  PERIOD2/A1 = IF ((TRN_DATE GE &&FXDATE.EVAL) AND (TRN_DATE LE &&TXDATE.EVAL)) 
             THEN 'Y' ELSE 'N';
  TCOMM/D12.2S = IF PERIOD2 EQ 'Y' THEN COMM_SEG ELSE 0;
  FARE/D12.2S = IF PERIOD2 EQ 'Y' THEN CP_FARE_PD ELSE 0;
  GFARE/D16.2CS = IF PERIOD2 EQ 'Y' THEN PAID_FAREX ELSE 0;
  MILES/D9C = IF PERIOD2 EQ 'Y' THEN MILES ELSE 0;
  KILO/D9C = IF PERIOD2 EQ 'Y' THEN KILO ELSE 0;
  NCOMM/D12.2S = IF PERIOD2 EQ 'Y' THEN NET_COMM ELSE 0;
  TICK_SEG/D8CS = IF PERIOD2 EQ 'Y' THEN NET_TKT_CNT ELSE 0;
  TFARE/D12.2S = IF PERIOD2 EQ 'Y' THEN TOTAL_FARE ELSE 0;
  GTFARE/D16.2CS = IF PERIOD2 EQ 'Y' THEN FARE_TOTX ELSE 0;
  ACOMM/D12.2S = IF PERIOD2 EQ 'Y' THEN COMM_SEG ELSE 0;
  AFARE/D12.2S = IF PERIOD2 EQ 'Y' THEN CP_FARE_PD ELSE 0;
  GAFARE/D16.2CS = IF PERIOD2 EQ 'Y' THEN PAID_FAREX ELSE 0;
  ANET/D12.2S = IF PERIOD2 EQ 'Y' THEN NET_COMM ELSE 0;
  ATICK_SEG/D8CS = IF PERIOD2 EQ 'Y' THEN NET_TKT_CNT ELSE 0;
  ATFARE/D12.2S = IF PERIOD2 EQ 'Y' THEN TOTAL_FARE ELSE 0;
  GATFARE/D16.2CS = IF PERIOD2 EQ 'Y' THEN FARE_TOTX ELSE 0;
  LABEL1/A25 = IF '&&ORGDST' EQ 'DIR_CITY_CD' THEN DIR_CTY_CDO ELSE
               IF '&&ORGDST' EQ 'DIR_CITY_NM' THEN DIR_CTY_NMO ELSE
               IF '&&ORGDST' EQ 'DIR_CP_CD' THEN DIR_APT_CDO ELSE
               IF '&&ORGDST' EQ 'DIR_CP_NM' THEN DIR_APT_NMO ELSE
               IF '&&ORGDST' EQ 'ND_CITY_CD' THEN ND_CTY_CDO ELSE
               IF '&&ORGDST' EQ 'ND_CITY_NM' THEN ND_CTY_NMO ELSE
               IF '&&ORGDST' EQ 'ND_CP_CD' THEN ND_APT_CDO ELSE
               IF '&&ORGDST' EQ 'ND_CP_NM' THEN ND_APT_NMO ELSE ' ';

 LABEL2/A25 = IF '&&ORGDST' EQ 'DIR_CITY_CD' THEN DIR_CTY_CDD ELSE
               IF '&&ORGDST' EQ 'DIR_CITY_NM' THEN DIR_CTY_NMD ELSE
               IF '&&ORGDST' EQ 'DIR_CP_CD' THEN DIR_APT_CDD ELSE
               IF '&&ORGDST' EQ 'DIR_CP_NM' THEN DIR_APT_NMD ELSE
               IF '&&ORGDST' EQ 'ND_CITY_CD' THEN ND_CTY_CDD ELSE
               IF '&&ORGDST' EQ 'ND_CITY_NM' THEN ND_CTY_NMD ELSE
               IF '&&ORGDST' EQ 'ND_CP_CD' THEN ND_APT_CDD ELSE
               IF '&&ORGDST' EQ 'ND_CP_NM' THEN ND_APT_NMD ELSE ' ';
END
-RUN

-IF &&CURRENCY EQ 'USD' THEN GOTO NEXT;
-SET &&SUBHEAD = <NAMEC;
-SET &GNET_CUR = 'TOTAL,NET,FARE, (' | &&CURRENCY || ')';
-SET &GALN_CUR= 'ALN,NET,FARE, (' | &&CURRENCY || ')';
-SET &GASHR_CUR= 'ALN,SHARE, (' | &&CURRENCY || ')';
-SET &&S_SUBJ6='COMPUTE MTOTREV/D16.2S=GMFARE+GMAFARE;'; 
-SET &&S_SUBJ7='AS ' | '''&GNET_CUR.EVAL''' ; 
-SET &&S_SUBJ8='GMFARE/D16.2CS AS ' | '''&GALN_CUR.EVAL'''  ; 
-SET &&S_SUBJ9='COMPUTE MSHR/D6.1S=IF ((MTOTREV LT 1) AND (MTOTSEG GT (-1)))';
-SET &&S_SUBJ10='THEN 0 ELSE ((GMFARE/MTOTREV)*100);'; 
-SET &&S_SUBJ11='AS ' | '''&GALN_CUR.EVAL'''; 
-SET &&S_SUBJ16='COMPUTE TOTREV/D10.2S=GFARE+GAFARE;';
-SET &&S_SUBJ17='AS ' | '''&GNET_CUR.EVAL'' GFARE/D16.2CS ';  
-SET &&S_SUBJ18='AS ' | '''&GALN_CUR.EVAL'' COMPUTE SHR/D6.1= ';
-SET &&S_SUBJ19='IF ((TOTREV LT 1) AND (TOTREV GT (-1))) THEN 0 ELSE';
-SET &&S_SUBJ20='((GFARE/TOTREV)*100); AS ' | '''&GASHR_CUR.EVAL'''; 

-NEXT;

