SET ASNAMES = ON
-RUN

-SET &&IDLEVELBREAK = '';

 
-SET &RPARM = &&WEB_PATH || 'RPTPARMS.fex';
 
-SET &AIRDATES = &&WEB_PATH || 'AIRDATES.fex';

-INCLUDE IDLEVROLLDEF
-RUN
 
DEFINE FILE &&EXTRACT
 
  BOOK_DT/MDYY         = RESV_DATE;
  BKD_ONLINE/A1 = IF ONLINE_TRADITIONAL EQ 'ONLINE' THEN 'Y' ELSE 'N';
  CHG_COD/A47 = IF EMP_ID NE ' ' AND LEVEL2 NE ' ' THEN
                LEVEL1  || '-' || LEVEL2 || '-' ||EMP_ID ELSE
                IF EMP_ID EQ ' ' AND LEVEL2 NE ' ' THEN
                LEVEL1  || '-' || LEVEL2 ELSE
                IF EMP_ID EQ ' ' AND LEVEL2 EQ ' ' THEN LEVEL1;
  
-*  CLASS_CAT/A8 = DECODE CL_CAT ('F' 'FIRST'     'C' 'COACH'
-*                              'D' 'DISCOUNT'  'B' 'BUSINESS');
  CLASS_CAT/A15 = DECODE CL_CAT ('F' 'FIRST'     'P' 'PREMIUM ECONOMY'   
			        'D' 'DEFAULT'   'B' 'BUSINESS'
			        'E' 'ECONOMY'   'U'  'UPGRADE' 
                                ' ' 'DEFAULT'); 
  FARE_PAID/D12.2C   = SEG_AMT + SEG_TAX;
  FLAG/A1 = 'A';
  IFLAG/A42 = DOD_DESC;

  GEO1/A3 =  EDIT(TKT_MAIN.GEO_ZONE,
  '999$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$');
  GRP_ADV_PUR1/A8    = IF ADV_PURCH LT 7 THEN '0 - 6' ELSE
                       IF ADV_PURCH GE 7 AND ADV_PURCH LE 13
                        THEN '07 - 13' ELSE
                      IF ADV_PURCH GE 14 AND ADV_PURCH LE 20
                         THEN '14 - 20' ELSE
                      IF ADV_PURCH GE 21 AND ADV_PURCH LE 30
                         THEN '21-30' ELSE '31 +';                  
  GRP_ADV_PUR/A8    = IF ADV_PURCH LT 7 THEN '0 - 6' ELSE
                      IF ADV_PURCH GE 7 AND ADV_PURCH LE 13
                        THEN '07 - 13' ELSE
                      IF ADV_PURCH GE 14 AND ADV_PURCH LE 20
                         THEN '14 - 20' ELSE
                      IF ADV_PURCH GT 20 THEN '21 +';                     
  KB1ST_BUS/A1      = IF ((AIR_MAIN.INTL_DOM EQ 'D') AND
                         (ADIR_ALL CONTAINS '*B' OR '*F') OR
                         (AIR_MAIN.INTL_DOM EQ 'I') AND
                         (ADIR_ALL CONTAINS '*B' OR '*F')) THEN 'Y' ELSE 'N';
  LEVEL_DESC1/A80 = &&LDESC;
  MC_ADV_BOOK/A8    = IF ADV_BOOK LT 7 THEN '0 - 6' ELSE
                      IF ADV_BOOK GE 7 AND ADV_BOOK LE 13 THEN '07 - 13' ELSE
                      IF ADV_BOOK GE 14 AND ADV_BOOK LE 20 THEN '14 - 20' ELSE
                      IF ADV_BOOK GT 20 THEN '21 +';
  MRK_TKT_CNT/D8CS    = IF (TICKET_CODE EQ '1') AND
                           (FARE_PAID GE 0) AND
                           (SEG_COUNT EQ 1) AND
                           (CONJ_REL EQ 1) THEN 1 ELSE
                        IF (TICKET_CODE EQ '1') AND
                           (EX_FLAG NE ' ') THEN (-1) ELSE
                        IF (TICKET_CODE EQ '1') AND
                           (RF_FLAG NE ' ') THEN (-1) ELSE 0;
 
  MX_ADV_PUR/A8       = IF (ADV_PURCH LT 7) THEN '0 - 6' ELSE
                        IF (ADV_PURCH LT 14) THEN '07 - 13' ELSE
                        IF (ADV_PURCH LT 21) THEN '14 - 20' ELSE '21 +';
  KM_ADV_PUR/A8       = IF (ADV_PURCH LT 7) THEN '0 - 6' ELSE
                        IF (ADV_PURCH LT 14) THEN '07 - 13' ELSE
                        IF (ADV_PURCH LT 21) THEN '14 - 20' ELSE
                        IF (ADV_PURCH LT 30) THEN '21 - 29' ELSE
                        IF (ADV_PURCH LT 60) THEN '30 - 59' ELSE '60 +' ;
  KW_ADV_PUR/A8       = IF (ADV_PURCH LT 4) THEN '0 - 3' ELSE
                        IF (ADV_PURCH GE 4) AND (ADV_PURCH LE 10)
                           THEN '04 - 10' ELSE '11 +' ;
  AP_ADV_PUR/A8       = IF (ADV_PURCH LE 10) THEN '0 - 10' ELSE '11+';
  KWCTY/A7 = SUBSTR(176, ANON_STP, 1, 6, 176, KWCTY);
 
  MX1ST_DOM/A1     =  IF (AIR_MAIN.INTL_DOM EQ 'D') AND
                         (FARE_PAID GE 500 OR FARE_PAID LE -500) AND
                         (CL_CAT EQ 'F') THEN 'Y' ELSE 'N';
 MX1ST_INT/A1    =    IF (AIRLINE EQ 'UA' AND CLASS EQ 'D') THEN 'N' ELSE
                         IF (CITYPAIR.INTL_DOM EQ 'I') AND
                         (FARE_PAID GE 1000 OR FARE_PAID LE -1000) AND
                         (CL_CAT EQ 'F') AND
                         (AIRLINE NE 'CO' OR 'IB')  THEN 'Y' ELSE 'N';
  MX1ST_FLAG/A1    = IF (AIR_MAIN.INTL_DOM EQ 'I') AND
                        (CITYPAIR.INTL_DOM EQ 'D') THEN 'X' ELSE ' ';
 NORG_APT/A25 = ND_ORG;
 NDST_APT/A25 = ND_DST;

  NET_TKT_CNT/D8CS    = IF (TICKET_CODE EQ '1') AND
                           (SEG_NBR EQ '01') AND
                           (FARE_PAID GE 0) AND
                           (SEG_COUNT EQ 1) AND
                           (CONJ_REL EQ 1) THEN 1 ELSE
                        IF (TICKET_CODE EQ '1') AND
                           (SEG_NBR EQ '01') AND
                           (EX_FLAG EQ 'F') THEN (-1) ELSE
                        IF (TICKET_CODE EQ '1') AND
                           (SEG_NBR EQ '01') AND
                           (RF_FLAG EQ 'F') THEN (-1) ELSE 0;
  NEW_ADV_PURCH/D9    = ADV_PURCH;
  NEW_ADV_PURCH1/D9    = IF SEG_NBR EQ '01' THEN ADV_PURCH ELSE 0;

  NEW_ADV_BOOK/D9     = ADV_BOOK;
  NON_REF/A15 = DECODE NONREF_FLAG ('N' 'NON-REFUNDABLE'
                                     'R' 'REFUNDABLE'
                                   ' ' 'REFUNDABLE');
  OTFLAG/A15 = IF DOD_TYPE EQ 'H' OR 'I' OR 'J' OR 'M' OR 'X' OR
                    'A' OR 'L' OR 'N' OR 'S' THEN 'ONLINE' ELSE 'TRADITIONAL';
  RICJOB/A45 = LEVEL1||LEVEL2||EMP_ID;
  REF_EXCH_CNT/D8CS   = IF (TICKET_CODE EQ '1') AND
                           (SEG_NBR EQ '01') AND
                           (FARE_PAID GE 0) AND
                           (SEG_COUNT EQ 1) AND
                           (CONJ_REL EQ 1) THEN 0 ELSE
                        IF (TICKET_CODE EQ '1') AND
                           (SEG_NBR EQ '01') AND
                           (EX_FLAG EQ 'F') THEN 1 ELSE
                        IF (TICKET_CODE EQ '1') AND
                           (SEG_NBR EQ '01') AND
                           (RF_FLAG EQ 'F') THEN 1 ELSE 0;
  SAVINGS/D11.2CS     = SEG_STANDARD - FARE_PAID;
  TKT_PURCH/D8CS      = IF (TICKET_CODE EQ '1') AND
                           (SEG_NBR EQ '01') AND
                           (FARE_PAID GE 0) AND
                           (SEG_COUNT EQ 1) AND
                           (CONJ_REL EQ 1) THEN 1 ELSE 0;
  TKT_PUR_NEX/P8CS = IF EXCHG_SALE EQ ' ' THEN TKT_PURCH ELSE 0; 
  TKT_PUR_EX/P8CS = IF EXCHG_SALE NE ' ' THEN TKT_PURCH ELSE 0;                         
 
  TKT_TYPE/A4          = IF (SEG_COUNT LE 0) AND
                          (RF_FLAG EQ 'P') THEN 'P-RF' ELSE
                        IF (SEG_COUNT LE 0) AND
                          (RF_FLAG EQ 'F') THEN 'F-RF' ELSE
                        IF (SEG_COUNT LE 0) AND
                          (EX_FLAG EQ 'P') THEN 'P-EX' ELSE
                        IF (SEG_COUNT LE 0) AND
                          (EX_FLAG EQ 'F') THEN 'F-EX' ELSE ' ';
                          
-*  NEW_ADV_PURCH/D9    = IF TKT_TYPE EQ 'F-EX' THEN (ADV_PURCH*(-1))
-*  						ELSE IF RF_FLAG EQ ' ' THEN ADV_PURCH
-*                        ELSE IF EX_FLAG EQ ' ' THEN ADV_PURCH  
-*                        ELSE (ADV_PURCH*(-1));
                        
  TRP_LEN/D9CS        = TRIP_LENGTH * SEG_COUNT;
  UP_ADV_PUR/A8    = IF ADV_PURCH LT 8 THEN '0 - 7' ELSE
                      IF ADV_PURCH GE 8 AND ADV_PURCH LE 14 THEN
                      '08 - 14' ELSE
                      IF ADV_PURCH GE 15 AND ADV_PURCH LE 21 THEN
                      '15 - 21' ELSE
                      IF ADV_PURCH GT 21 THEN '22 +';
  VASAVINGS/D11.2CS   = SEG_DISCOUNT - FARE_PAID;
  XAP/A8 = EDIT(ADV_PURCH);
  XAB/A8 = EDIT(ADV_BOOK);
  XARR_DOW/W           = ARR_DATE;
  XARR_DT/MDYY         = ARR_DATE;
  XCRDT_CD/A18 = 'AX9999999999999999';
  XDPT_DOW/W           = DPT_DATE;
  XDPT_DT/MDYY         = DPT_DATE;
  XPSNGR_NM/A15        =
  EDIT(PASSNGR_NAME,'999999999999999$$$$$$$$$$$$$$$$$$');
  XTRAN_DT/MDYY        = &&WHATDATE;
 
 
-* ****DEFINES ON PREVIOUS DEFINES****
  AIRLINE_FEE/D12.2CS   = IF EMBARK EQ 'FP2' OR 'FP3' THEN FARE_PAID ELSE 0;
  ARR_DAY/A3          = DECODE XARR_DOW (1 'MON' 2 'TUE' 3 'WED'
                                 4 'THU' 5 'FRI' 6 'SAT' 7 'SUN');
  DISSAVINGS/D11.2CS  = IF EMBARK EQ 'DT1' THEN 0 ELSE
                         FARE_PAID - SEG_LOWEST;
  DPT_DAY/A3          = DECODE XDPT_DOW (1 'MON' 2 'TUE' 3 'WED'
                                 4 'THU' 5 'FRI' 6 'SAT' 7 'SUN');
  FARE1/D12.2C = FARE_PAID;                                 
  FARE2/D12.2C = IF EMBARK NE 'DT1' THEN FARE_PAID ELSE 0;
  FARE3/D12.2C = IF (EMBARK NE 'DT1' OR 'RF1' OR 'FP2' OR 'FP3' OR 'EX1' OR 'EX5') AND (EX_FLAG NE 'F' OR 'P') AND (RF_FLAG NE 'P') THEN FARE_PAID ELSE 0;  
  NETT1/P8CS = NET_TKT_CNT;
  NETT2/P8CS = IF EMBARK NE 'DT1' THEN NET_TKT_CNT ELSE 0;
  NETT3/P8CS = IF (EMBARK NE 'DT1' OR 'RF1' OR 'FP2' OR 'FP3' OR 'EX1' OR 'EX5') AND (EX_FLAG NE 'F' OR 'P') AND (RF_FLAG NE 'P') THEN NET_TKT_CNT ELSE 0;
  MX_DAYS/A8 =  IF ADV_PURCH LT 21 THEN XAP ELSE MX_ADV_PUR;
  KM_DAYS/A8 =  IF ADV_PURCH LT 60 THEN XAP ELSE KM_ADV_PUR;
  UP_DAYS/A8 =  IF ADV_PURCH LT 22 THEN XAP ELSE UP_ADV_PUR;
  MC_DAYS/A8 =  IF ADV_BOOK LT 21 THEN XAB ELSE MC_ADV_BOOK;
  BB_06/D8CS = IF MX_ADV_PUR EQ '0 - 6' THEN TKT_PURCH ELSE 0;
  FIRSTCL/D8CS = IF (MX1ST_FLAG EQ ' ') AND
                    (MX1ST_DOM EQ 'Y') THEN 1 ELSE
                 IF (MX1ST_FLAG EQ ' ') AND
                    (MX1ST_INT EQ 'Y') THEN 1 ELSE 0;
  SPACER/A10 = 'DEPARTMENT';                    
-* Added for Gallup
  PJ_NO/A1 = EDIT(LEVEL2, '9');
-* added for Kohls department defines
-INCLUDE KODEPTS
-INCLUDE IDLEVDEFINE 
 	
END
-RUN
 
TABLEF FILE &&EXTRACT
-* Report column elements
  PRINT AGENT_NUM
        AIR_KEY
        AIR_MAIN.INTL_DOM
        AIRLINE
        AIRLINE_FEE
        AIRLINE_NAME
        AP_ADV_PUR
        AR_BRANCH
        ARR_DAY
        ARR_TIME
        AROLL_DSC3
        AROLL_DSC4
        AROLL_DSC5
        AROLL_DSC7
        BB_06
        BKD_ONLINE
        BOOK_DT
        BR_CL_IDX
        C_ITIN
        CASH_CREDIT
        CHG_COD
        CITYPAIR.EMBARK      AS 'EMB_APT_CD'
        CITYPAIR.ROUTE       AS 'RTE_APT_CD'
        CITYPAIR.INTL_DOM AS 'C_INTDOM'
        CLASS
        CLASS_CAT
        CLIENT_NAME
        CONJ_REL
        CREDIT_CARD
        DISSAVINGS
        DOC_TYPE
        DOC_NUMBER
        DOD_TYPE
        DPT_DAY
        DPT_TIME
        EMB_APT.AIRPORT_NAME AS 'EMB_APT_NM'
        EMB_CTY.CITY_NAME    AS 'EMB_CTY_NM'
        EMB_CTY.EMB_CITY     AS 'EMB_CTY_CD'
        FARE_BAS
        FARE_PAID
        FARE1
        FARE2
        FARE3
        FIRSTCL
        FLAG
        FLIGHT
        GRP_ADV_PUR
        GRP_ADV_PUR1
        GSA_GTR
        IFLAG
        INVOICE_NUM
        KB1ST_BUS
        LEVEL_DESC
        EMP_ID
        LEVEL1
        LEVEL2
        LEVEL3
        KM_DAYS
        KM_ADV_PUR
        KW_ADV_PUR
        KO_DPT
        MC_ADV_BOOK
        MC_DAYS
        MRK_TKT_CNT
        MX_ADV_PUR
        MX_DAYS
        MX1ST_DOM
        MX1ST_FLAG
        MX1ST_INT
        NORG_APT
        NDST_APT
        NET_TKT_CNT
        NETT1
        NETT2
        NETT3
        NEW_ADV_BOOK
        NEW_ADV_PURCH
        NEW_ADV_PURCH1
        NONREF_FLAG
        NON_REF
        ORG_DST
        OTFLAG
        PNR_LOCATOR
        RICJOB
        REF_EXCH_CNT
        REFUSAL_DESC
        REFUSE_CODE
        REFUSE_KEY
        RTE_APT.AIRPORT_NAME AS 'RTE_APT_NM'
        RTE_CTY.CITY_NAME    AS 'RTE_CTY_NM'
        RTE_CTY.RTE_CITY     AS 'RTE_CTY_CD'
        SAVINGS
        SEG_AMT
        SEG_DESIG
        SEG_DISCOUNT
        SEG_LOWEST
        SEG_NBR
        SEG_STANDARD
        SEG_TAX
        SPACER
        STAY
        TICKET_BRANCH
        TKT_NUM
        TKT_PURCH
        TKT_PUR_EX
        TKT_PUR_NEX
        TKT_SORT
        TKT_TYPE
        TOUR_CODE
        TRIP_PURPOSE
        TRN_DATE
        FST_DPT_DATE
        TRP_LEN
        TRVL_AUTH
        UP_ADV_PUR
        UP_DAYS
        VAL_AIRLINE
        VASAVINGS
        XARR_DT
        XCRDT_CD
        XDPT_DT
        XPSNGR_NM
        XTRAN_DT
        RF_FLAG
        EX_FLAG

-INCLUDE BYHIERARCHY 
-INCLUDE &AIRDATES
 
WHERE VOID_DATE EQ 0
-********************************************************************
-* Where statements to select data specified in user profile
-********************************************************************
&&WHERE1
&&WHERE2
&&WHERE3
&&WHERE4
&&WHERE5
&&WHERE6
&&WHERE7
&&WHERE8
&&WHERE9
&&WHERE10
 
 
-INCLUDE &RPARM
 
ON TABLE HOLD AS &&RPT_HOLD
END
-RUN


-SET &&FindData = IF &LINES EQ 0 THEN 'N' ELSE 'Y';

