DEFINE FILE H1RUAN
REFUSE_KEYX/A10 = 'RUAN-R  ' | REFUSE_CODE;
END
TABLE FILE H1RUAN
PRINT REFUSE_KEYX
BY HTL_KEY
WHERE REFUSE_CODE NE ' ' OR 'B' OR 'D' OR 'L' 
ON TABLE HOLD AS HTLFIX
END
-RUN

USE CLEAR *
-RUN

TABLE FILE HTLFIX
PRINT REFUSE_KEYX AS 'REFUSE_KEY'
BY HTL_KEY
ON TABLE HOLD AS FIXHTL
END
-RUN

MODIFY FILE H1RUAN
LOG NOMATCH MSG OFF
FIXFORM FROM FIXHTL
MATCH HTL_KEY
ON MATCH UPDATE REFUSE_KEY
ON NOMATCH REJECT
DATA ON FIXHTL
END
-RUN

