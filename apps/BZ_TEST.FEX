SET ASNAMES = ON


DEFINE FILE SZBOOZ
  BZ_UP1/A1 = IF SEG_NBR EQ '01' AND FARE_BAS CONTAINS 'UP' THEN 'Y' ELSE 'N';
  BZ_UP2/A1 = IF SEG_NBR EQ '02' AND FARE_BAS CONTAINS 'UP' THEN 'Y' ELSE 'N';
  BZ_UP3/A1 = IF SEG_NBR EQ '03' AND FARE_BAS CONTAINS 'UP' THEN 'Y' ELSE 'N';
  BZ_UP4/A1 = IF SEG_NBR EQ '04' AND FARE_BAS CONTAINS 'UP' THEN 'Y' ELSE 'N';
  BZ_UP/A1 = IF BZ_UP1 EQ 'Y' OR BZ_UP2 EQ 'Y' OR BZ_UP3 EQ 'Y' OR BZ_UP4 EQ 'Y' THEN 'Y' ELSE 'N';
  
END
TABLE FILE SZBOOZ
SUM BZ_UP1 BZ_UP2 BZ_UP3 BZ_UP4 BZ_UP
BY AIR_KEY
IF AIR_KEY CONTAINS 7836471936
END
-RUN
