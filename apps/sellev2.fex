FILEDEF FILE01 DISK d:\tnt\FILE01.TXT
-RUN
TABLE FILE FILE01
PRINT LEV
BY FX
ON TABLE HOLD AS HLDSF FORMAT FOCUS INDEX FX
END
-RUN

JOIN GLOBAL_PARM IN RPT_INST TO FX IN HLDSF
-RUN

DEFINE FILE RPT_INST
ILEV/I2=EDIT(LEV);
END
TABLE FILE RPT_INST
PRINT INST_KEY ILEV AS SELECT_LEV
WHERE SELECT_LEV NE ILEV
WHERE LEV NE ' '
ON TABLE SET ASNAMES ON
ON TABLE HOLD AS IHOLD
END
MODIFY FILE RPT_INST
FIXFORM FROM IHOLD
MATCH INST_KEY 
 ON NOMATCH REJECT
 ON MATCH UPDATE SELECT_LEV
DATA ON IHOLD
END

