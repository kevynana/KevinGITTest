-INCLUDE SETECHO

SET ASNAMES = ON


DEFINE FILE AZKIEW
NEWTRP/A10 = 'KIEWIT-R' || TRVLCD ;
END
TABLE FILE AZKIEW
PRINT NEWTRP AS 'TRP_KEY'
BY AIR_KEY
IF BR_CL_IDX CONTAINS 999999
ON TABLE HOLD
END
-RUN

MODIFY FILE AZKIEW
FIXFORM FROM HOLD
MATCH AIR_KEY
ON MATCH UPDATE TRP_KEY
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN
