SET ASNAMES = ON;

DEFINE FILE HYKIEW
RFKEY1/A2 = EDIT(REFKEY, '99');
RFKEY2/A8 = '        ';
RFKEY4/A10 = RFKEY2 || RFKEY1;
RFKEY3/A10 = IF REFKEY EQ ' ' THEN ' ' ELSE RFKEY4;
END
TABLE FILE HYKIEW
PRINT HTL_KEY RFKEY3 AS 'REFKEY'
BY HTL_KEY NOPRINT
BY RFKEY3 NOPRINT
IF BR_CL_IDX EQ 'KW999999'
ON TABLE HOLD AS DEBTEST
END
-RUN

MODIFY FILE HYKIEW
FIXFORM FROM DEBTEST
MATCH HTL_KEY
ON MATCH UPDATE REFKEY
ON NOMATCH REJECT
DATA ON DEBTEST
END
-RUN
