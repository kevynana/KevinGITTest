SET ASNAMES = ON


-INCLUDE SETECHO


DEFINE FILE A6IRC
LV4/A60 = INT_CODE;
END
TABLE FILE A6IRC
PRINT AIR_KEY LV4
BY AIR_KEY NOPRINT
IF LEVEL4 EQ ' '
IF INT_CODE NE ' '
ON TABLE HOLD
END
-RUN

TABLE FILE HOLD
PRINT LV4 AS 'LEVEL4'
BY AIR_KEY
ON TABLE HOLD AS AIRHOLD
END
-RUN

MODIFY FILE A6IRC
FIXFORM FROM AIRHOLD
MATCH AIR_KEY
ON MATCH UPDATE LEVEL4
ON NOMATCH REJECT
DATA ON AIRHOLD
END
-RUN


