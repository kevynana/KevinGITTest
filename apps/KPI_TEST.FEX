

-INCLUDE SETECHO
SET ASNAMES = ON 
 
-DEFAULTH &BACKCOLOR = 'rgb(255, 255, 255)';

-DEFAULTH &WF_TITLE = 'TITLE';
-SET &WF_SUMMARY = 'SUMMARY';

DEFINE FILE CAR
SALES2/D12M = SALES;
END

TABLE FILE CAR
HEADING CENTER
" "
"Open Orders"
"<SALES2"
SUM
DEALER_COST AS 'Booking $' OVER
RETAIL_COST AS 'Booking/Open %'  OVER
SALES2 AS 'Cubic Volume'  OVER
ON TABLE PCHOLD FORMAT HTML
ON TABLE NOTOTAL
ON TABLE SET PAGE-NUM NOLEAD
ON TABLE SET SQUEEZE ON
ON TABLE SET HTMLCSS ON
ON TABLE SET HTMLENCODE OFF
ON TABLE PCHOLD FORMAT PDF
ON TABLE SET STYLE *
-*INCLUDE=IBFS:/FILE/IBI_HTML_DIR/javaassist/intl/EN/ENIADefault_combine.sty,$
TYPE=REPORT, TITLETEXT=&WF_TITLE.QUOTEDSTRING, SUMMARY=&WF_SUMMARY.QUOTEDSTRING, GRID=OFF, BORDER=ON, BACKCOLOR=NONE, $

TYPE=HEADING, LINE=1, COLOR=BLACK, BORDER=ON, SIZE=10, $
TYPE=HEADING, LINE=2, COLOR=BLACK, BORDER=ON, SIZE=10, $
TYPE=HEADING, LINE=3, COLOR=BLACK, BORDER=ON, SIZE=10, $
TYPE=HEADING, LINE=4, COLOR=BLACK, BORDER=ON, $

TYPE=TITLE, COLOR=BLACK, BORDER=OFF, JUSTIFY=CENTER, BACKCOLOR=RED, SIZE=8, $
TYPE=DATA, COLOR=BLACK, BORDER=OFF, JUSTIFY=CENTER, BACKCOLOR=NONE, SIZE=8, $
ENDSTYLE
END
-RUN
-QUIT

-SET &IBIAPPS = GETTOK(&FOCEXURL,&FOCEXURL.LENGTH,1,'/',64,'A64');
-SET &IBIAPPS = TRUNCATE(&IBIAPPS);
-HTMLFORM BEGIN
<script type="text/javascript" src="./jquery/js/jquery.min.js"></script>
<script>
$('body').css( { 'background-color':'!IBI.AMP.BACKCOLOR;' } );
$('body > table').width( '100%' );
$('.x6').width( '33%' );
</script>
-HTMLFORM END  


 