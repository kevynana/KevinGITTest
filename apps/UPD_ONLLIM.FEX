SET ASNAMES = ON


 
TABLE FILE LZBOOZ
PRINT CAR_KEY   
BY CAR_KEY NOPRINT
IF DOD_TYPE EQ 'L'
ON TABLE HOLD
END

MODIFY FILE LZBOOZ
FIXFORM FROM HOLD
MATCH CAR_KEY
ON MATCH COMPUTE DOD_TYPE='K';
ON MATCH UPDATE DOD_TYPE
ON NOMATCH REJECT
DATA ON HOLD
END


TABLE FILE LZBOOZ
PRINT CAR_KEY   
BY CAR_KEY NOPRINT
IF DOD_TYPE EQ 'S'
ON TABLE HOLD
END

MODIFY FILE LZBOOZ
FIXFORM FROM HOLD
MATCH CAR_KEY
ON MATCH COMPUTE DOD_TYPE=' ';
ON MATCH UPDATE DOD_TYPE
ON NOMATCH REJECT
DATA ON HOLD
END
