DEFINE FILE AIRCARI
NAMEC/A30 = CNAME;

-******************************************************************************
-*                         Global Currency Conversion Defines
-******************************************************************************
CURR1X/D20.6 = 1 / USDPU ;

-* DEFINES ON PREVIOUS DEFINES
PAID_FAREX/D20.6C = FARE_PAID * CURR1X;
PAID_FARE1/D20.6C = PAID_FAREX;
FARE_BASEX/D20.6C = BASE_FARE * CURR1X;
FARE_BASE1/D20.6C = FARE_BASEX;
FARE_TOTALX/D20.6C = TOTAL_FARE * CURR1X;
FARE_TTL1/D20.6C = FARE_TOTALX;	
TAX_TOTALX/D20.6C = TOTAL_TAX * CURR1X;
TAX_TTL1/D20.6C = TAX_TOTALX;
NFVSVX/D20.6 = NFVSV*CURR1X;

END

