EX HTLUSE
-RUN

-TYPE Before Extract
SET ASNAMES = ON
-SET HOLDATTR = ON;
-RUN

DEFINE FILE &&EXTRACT
CITY_ST/A35 = PROP_CITY||(', '| PROP_STATE);
IN_DATE/MDYY = CHKIN_DATE;
INV_DT/YYMD = INVOICE_DATE;
NBR_DAYS/P12 = NUM_DAYS;
NBR_ROOMS/P12 = NUM_ROOMS;
NEW_ROOM_RATE/P12.2C = ROOM_RATE;
NEW_TOTAL_AMT/P12.2C = TOTAL_AMT;
NNROOMS/P12 = IF NBR_DAYS LT 0 AND NUM_ROOMS LT 0 THEN
 ((NBR_DAYS*NUM_ROOMS) * (-1)) ELSE NBR_DAYS*NUM_ROOMS;
OUT_DATE/MDYY = CHKOUT_DATE;
REASON_CD/A5 = IF HTL_REF.REFUSE_KEY EQ 'UPTTL-R A' THEN
                'CLC' ELSE 'OTHER';
END
-RUN

TABLEF FILE &&EXTRACT
PRINT 
  CITY_ST     
  HTL_KEY     
  IN_DATE 	
  INV_DT      
  INVOICE_DATE 	
  NBR_DAYS    
  NBR_ROOMS
  NEW_ROOM_RATE
  NEW_TOTAL_AMT       
  NNROOMS	
  OUT_DATE    
  PASSNGR_NAME 	
  PROP_NAME   
  REFUSE_KEY
   
    
