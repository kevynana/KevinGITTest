  NEW_SEG_MILES
  NHR_REF.NHR_DESC
  NOHTL_KEY
  PASSNGR_NAME
  PAID_FARE
  REF_EXCH_CNT
  REFUSE_CODE
  REFUSAL_DESC
  RTE_APT.AIRPORT_NAME AS 'RTE_APT_NM'	
  RTE_CTY.CITY_NAME    AS 'RTE_CTY_NM'
  SAVINGS
  SEG_AMT
  SEG_COM
  SEG_COUNT
  SEG_LOWEST
  SEG_MILES
  SEG_NBR
  SEG_TAX
  TKT_AMOUNT
  TKT_DATE
  TKT_NUM
  TKT_PURCH	
  TKT_SORT
  TKT_TYPE	
  TRN_DATE
  TRP_REF.TRP_DESC
  TRIP_RSN
  TRIPRSN
  VOID_DATE
  XDPT_DT
  XDPT_DATE	
  XPSNGR_NM
  XTRAN_DT
  
 

-INCLUDE &AIRDATES


-********************************************************************
-* Where statements to select data specified in user profile
-********************************************************************
&&WHERE1
&&WHERE2
&&WHERE3
&&WHERE4
&&WHERE5
&&WHERE6
&&WHERE7
&&WHERE8
&&WHERE9
&&WHERE10


