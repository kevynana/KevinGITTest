-* Global Reporting JOIN
JOIN GCAIR2.TKT_DATE IN GCAIR2 TO GLOBAL.DATE IN GLOBAL AS J1
END

DEFINE FILE GCAIR2
NAMEC/A30 = CNAME;

-******************************************************************************
-*                         Global Currency Conversion Defines
-******************************************************************************
CURR1X/D20.6 = 1 / USDPU ;

-* DEFINES ON PREVIOUS DEFINES
PAID_FAREX/D20.6C = FARE_PAID * CURR1X;
PAID_FARE1/D20.6C = PAID_FAREX;
DISSAVEX/D20.6 = DISSAVINGS * CURR1X;
SAVINGSX/D20.6 = SAVINGS * CURR1X;
DISSAVE1/D20.6 = DISSAVEX;
SAVINGS1/D20.6 = SAVINGSX;
SEG_LOWESTX/D20.6 = SEG_LOWEST * CURR1X;
SEG_LOWEST1/D20.6 = SEG_LOWESTX;
SEG_AMTX/D20.6 = NEW_SEG_AMT * CURR1X;
SEG_AMT1/D20.6 = SEG_AMTX;
SEG_TAXX/D20.6 = SEG_TAX * CURR1X;
SEG_TAX1/D20.6 = SEG_TAXX;
END


