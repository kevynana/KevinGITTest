-SET &WHERE1=IF &2 EQ 1 THEN 'WHERE USER_NAME EQ ''&3.EVAL''' ELSE '-*';


DEFINE FILE RPT_SET
SORT_ORDER/I1=IF USER_NAME EQ '&3' THEN 1 ELSE 2;
UN_DISPLAY/A20=IF USER_NAME EQ ' ' THEN 'UNASSIGNED' ELSE USER_NAME;
END
TABLE FILE RPT_SET
SUM USER_NAME 
BY SORT_ORDER NOPRINT
BY UN_DISPLAY
WHERE ROLLUP_CODE EQ '&1'
WHERE USER_SECURITY LE &2
&WHERE1.EVAL
ON TABLE HOLD
END

TABLE FILE HOLD
PRINT USER_NAME
      UN_DISPLAY
ON TABLE PCHOLD
END

