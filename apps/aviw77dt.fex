SET ASNAMES=ON
-RUN


TABLE FILE &&RPT_HOLD
SUM FARE_PAID AS 'TFARE'
    NET_TKT_CNT AS 'TTKT'
    NONREF_CNT AS 'NRTKT'
    NONREF_FARE AS 'NRFARE'
    REF_CNT AS 'RTKT'
    REF_FARE AS 'RFARE'
BY LEVEL_DESC
BY XPSNGR_NM
BY TKT_SORT
BY TRN_DATE
ON TABLE HOLD AS AIRHOLD1
END
-RUN

TABLE FILE &&RPT_HOLD
PRINT ABOOK_CNT
  ABOOK_FARE
