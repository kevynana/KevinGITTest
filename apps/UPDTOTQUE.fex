-*4/12/17-JEM-S-33208 Updated to be able to use in ER5

TABLE FILE RPT_QUE_SQL
PRINT INST_KEY INTIME
BY INST_KEY NOPRINT
WHERE RPT_SEQ EQ 999
ON TABLE HOLD
END
-RUN

MODIFY FILE RPT_QUE_SQL
FIXFORM FROM HOLD
MATCH INST_KEY INTIME
ON MATCH COMPUTE EX_FLAG='Y';
ON MATCH UPDATE EX_FLAG
ON NOMATCH REJECT
DATA ON HOLD
END
-RUN
