
-SET &INST_KEY = &&IKEY;
-RUN

TABLE FILE BRPTINST
PRINT PAIR_DB 
WHERE INST_KEY EQ &INST_KEY
ON TABLE SAVE AS PAIRDB
END
-RUN
-READ PAIRDB &PAIRDB.A1. 
-CLOSE PAIRDB


-SET &PAIRDB = '&PAIRDB.EVAL'

-IF &PAIRDB EQ 'S' THEN GOTO SEG ELSE GOTO MRK;
-SEG;
-SET &TTL1 = 'SEG,CNT';
-SET &&PRINT10 = 'NET_TKT_CNT AS ''&TTL1.EVAL''';
-GOTO NEXT1;
-MRK;
-SET &TTL1 = 'MKT,CNT';
-SET &&PRINT10 = 'NET_TKT_CNT AS ''&TTL1.EVAL''';

-NEXT1;
