-          IF &&OUTLINE2 CONTAINS 'EXCEL' THEN 'EXCEL' ELSE
-          IF &&OUTLINE2 CONTAINS 'EXL2K' THEN 'EXL2K' ELSE 'PDF';
 
 
-****
-* &&MFLAG WILL EQUAL ONE OF THE FOLLOWING:
-* JAN, FEB, MAR, APR, MAY, JUN, JUL, AUG, SEP, OCT, NOV, DEC, YTD
-****
 
 
-IF &&MFLAG EQ 'JAN' THEN GOTO JAN
-  ELSE IF &&MFLAG EQ 'FEB' THEN GOTO FEB
-  ELSE IF &&MFLAG EQ 'MAR' THEN GOTO MAR
-  ELSE IF &&MFLAG EQ 'APR' THEN GOTO APR
-  ELSE IF &&MFLAG EQ 'MAY' THEN GOTO MAY
-  ELSE IF &&MFLAG EQ 'JUN' THEN GOTO JUN
-  ELSE IF &&MFLAG EQ 'JUL' THEN GOTO JUL
-  ELSE IF &&MFLAG EQ 'AUG' THEN GOTO AUG
-  ELSE IF &&MFLAG EQ 'SEP' THEN GOTO SEP
-  ELSE IF &&MFLAG EQ 'OCT' THEN GOTO OCT
-  ELSE IF &&MFLAG EQ 'NOV' THEN GOTO NOV
-  ELSE IF &&MFLAG EQ 'DEC' THEN GOTO DEC
-  ELSE GOTO YTD;
 
-JAN
-SET &&MCHK='WHERE TRNM EQ ''JAN''';
-SET &&SUBHEAD = 'JANUARY';
-GOTO ENDDATE
 
-FEB
-SET &&MCHK='WHERE TRNM EQ ''FEB''';
-SET &&SUBHEAD = 'FEBRUARY';
-GOTO ENDDATE
 
-MAR
-SET &&MCHK='WHERE TRNM EQ ''MAR''';
-SET &&SUBHEAD = 'MARCH';
-GOTO ENDDATE
 
-APR
-SET &&MCHK='WHERE TRNM EQ ''APR''';
-SET &&SUBHEAD = 'APRIL';
-GOTO ENDDATE
 
-MAY
-SET &&MCHK='WHERE TRNM EQ ''MAY''';
-SET &&SUBHEAD = 'MAY';
-GOTO ENDDATE
 
-JUN
-SET &&MCHK='WHERE TRNM EQ ''JUN''';
-SET &&SUBHEAD = 'JUNE';
-GOTO ENDDATE
 
-JUL
-SET &&MCHK='WHERE TRNM EQ ''JUL''';
-SET &&SUBHEAD = 'JULY';
-GOTO ENDDATE
 
-AUG
-SET &&MCHK='WHERE TRNM EQ ''AUG''';
-SET &&SUBHEAD = 'AUGUST';
-GOTO ENDDATE
 
-SEP
-SET &&MCHK='WHERE TRNM EQ ''SEP''';
-SET &&SUBHEAD = 'SEPTEMBER';
-GOTO ENDDATE
 
-OCT
-SET &&MCHK='WHERE TRNM EQ ''OCT''';
-SET &&SUBHEAD = 'OCTOBER';
-GOTO ENDDATE
 
-NOV
-SET &&MCHK='WHERE TRNM EQ ''NOV''';
-SET &&SUBHEAD = 'NOVEMBER';
-GOTO ENDDATE
 
-DEC
-SET &&MCHK='WHERE TRNM EQ ''DEC''';
-SET &&SUBHEAD = 'DECEMBER';
-GOTO ENDDATE
 
-YTD
-SET &&MCHK='WHERE TRNY EQ ''YTD''';
-SET &&SUBHEAD = 'YEAR-TO-DATE';
-GOTO ENDDATE
 
 
-ENDDATE
 
 
-FIELDS
-IF &&FIELD EQ 'HOTEL BOOKINGS' THEN GOTO BOOKINGS
-  ELSE IF &&FIELD EQ 'HOTEL NIGHTS' THEN GOTO NIGHTS
-  ELSE IF &&FIELD EQ 'HOTEL COST' THEN GOTO COST
-  ELSE GOTO BOOKINGS;
 
-GOTO OUTSIDE
 
-BOOKINGS
-SET &&RPT_TTL1 = '                  HOTEL BOOKINGS DETAIL     ';
-GOTO OUTSIDE
 
 
-NIGHTS
