 PREF_NON/A15 = IF PNP_FLAG EQ 'I'  THEN 'PREFERRED' ELSE 'NON-PREFERRED';
 
NONP/A1 = IF PREF_NON EQ 'NON-PREFERRED' THEN 'Y' ELSE 'N';
-* NONP/A1 = IF REFUSE_CD NE 'A' OR 'N' OR 'S' OR 'M' OR 'T' OR 'R' THEN 'Y' ELSE 'N';
