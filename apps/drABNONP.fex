NBR_DAYS1/D12 = IF ((NBR_DAYS LT 0) AND (NBR_ROOMS LT 0)) THEN
                ((NBR_DAYS*NBR_ROOMS) * (-1)) ELSE NBR_DAYS*NBR_ROOMS;
OUT_DATE/MDYY = CHKOUT_DATE;
REFUSE_CD/A2=EDIT(REFUSE_KEY,'$$$$$$$$99');
