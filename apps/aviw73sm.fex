ON TABLE HOLD AS XTWO
END
-RUN

MATCH FILE XTWO
PRINT
  CLIENT_NAME
  CTRY_FLAG 
  DISSAVINGS
  FARE_PAID	
  GRP_ADV_PUR 	
  EMP_ID
  LEVEL1
  LEVEL2
  LEVEL3
