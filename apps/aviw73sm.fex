SET ASNAMES = ON


TABLE FILE &&RPT_HOLD

SUM 
    DISSAVINGS AS 'DISSAV'
    FARE_PAID/D12.2CS AS 'TFARE'
    NET_TKT_CNT AS 'NTTKT'
    REF_EXCH_CNT AS 'TRFEX'
    SAVINGS AS 'TSAVE'
    TKT_PURCH AS 'TTKT'
BY AROLL_DSC1    
ON TABLE HOLD AS HOLDONE
END
-RUN

