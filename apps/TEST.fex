-INCLUDE SETECHO
-SET &&TOTCOMPS  =1;
-SET &CNT = 1;
-SET &RECS1 = 1;
-SET &RECS3 = 1;

-SET &TEMPHOLD = IF (&RECS1+&RECS3) GT 0 THEN 'TEMPHOLD' ELSE 'HOLDUD02';
-*-SET &TEMPHOLD = IF &CNT EQ 1 THEN 'TEMPHOLD' ELSE 'HOLDUD02';


MATCH FILE UAWWF
BY AIR_KEY
WHERE UDID EQ 'PR#' AND AUD_DATA EQ '9Z136500'
RUN
FILE UAWWF
BY AIR_KEY
WHERE UDID EQ 'FSR' AND AUD_DATA EQ '1576'
AFTER MATCH HOLD OLD-OR-NEW
RUN
FILE UAWWF
BY AIR_KEY
WHERE UDID EQ 'PR#' AND AUD_DATA EQ '53500000'
AFTER MATCH HOLD OLD-OR-NEW
RUN
FILE UAWWF
BY AIR_KEY
WHERE UDID EQ 'FSR' AND AUD_DATA EQ '7350'
AFTER MATCH HOLD AS &TEMPHOLD OLD-OR-NEW
END
MATCH FILE UAWWF
BY AIR_KEY
RUN
FILE &TEMPHOLD
BY AIR_KEY
AFTER MATCH HOLD AS HOLD2 OLD-NOT-NEW
END
-RUN

-EXIT

TABLE FILE UAWWF
BY AIR_KEY
WHERE UDID EQ 'PR#' AND AUD_DATA EQ '53500000'
END
TABLE FILE SR_50
ON TABLE SET PAGE-NUM OFF
PRINT RTE_APT.GEO_ZONE AND RTE_APT.COUNTRY_CODE
HEADING
"PAGE <TABPAGENO  "
ON TABLE NOTOTAL
ON TABLE SET STYLE *
UNITS=IN, PAGESIZE='Letter', LEFTMARGIN=0.250000, RIGHTMARGIN=0.250000,
    TOPMARGIN=0.250000, BOTTOMMARGIN=0.250000, SQUEEZE=ON,
    ORIENTATION=PORTRAIT, $
TYPE=REPORT, GRID=OFF, FONT='TIMES NEW ROMAN', SIZE=10, COLOR=BLACK,
    BACKCOLOR=NONE, STYLE=NORMAL, $
ENDSTYLE
ON TABLE SET ONLINE-FMT HTML
END


TABLE FILE HR_50
ON TABLE SET PAGE-NUM OFF
