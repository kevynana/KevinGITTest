-* File push_users.fex
TABLE FILE CONTROL
PRINT ROLLUP_CODE
ON TABLE SAVE
END
-RUN
 
-READ SAVE &ROLL.A8
 
 
JOIN
 INNER PUSH_GROUPS.PUSH_GROUPS.SET_KEY IN PUSH_GROUPS TO UNIQUE
 RPT_SET.RPT_SET.SET_KEY IN RPT_SET AS J0
END
 
 
DEFINE FILE PUSH_GROUPS
SEL/A1=IF SCHED_OWNER EQ '&ETEKUSER' THEN 'Y' ELSE 'N';
END
TABLE FILE PUSH_GROUPS
SUM SCHED_OWNER SEL
BY SCHED_OWNER
WHERE ROLLUP_CODE EQ '&ROLL';
ON TABLE PCHOLD
END
