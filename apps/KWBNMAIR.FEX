TABLE FILE &&RPT_HOLD
PRINT BR_CL_IDX
  CARD
  CCARD2
  CGADV
  CRD_CRD
  CREDIT_CARD
  DECLINE_AMT
  DISSAVINGS
  DOC_TYPE	
  DOD_TYPE	
  EMBARK
  EMB_APT_CD
  EX_FLAG	
  EXCHG_SALE	
  FARE_PAID
  FLAG
  INTDOM
  ITDOM
  LEVEL_DESC
  LEVEL1
  NET_TKT_CNT
  NEW_SEG_MILES
  NONREF_FLAG
  MILES
  ONLINE_TRADITIONAL
  PASSNGR_NAME
  PFARE
  PMILES
  REFUSE_CODE
  RF_FLAG
  SAVINGS
