  TKT_SORT
  TRN_DATE
  TRAN_MY
  VASAVINGS
  VOID_DATE 	
  VOID_STATUS 
  XPSNGR_NM

WHERE (&ADTDESC GE &&FROMDT.EVAL) AND (&ADTDESC LE &&TODT.EVAL) 
WHERE VOID_DATE EQ 0
-********************************************************************
-* Where statements to select data specified in user profile
-********************************************************************
&&WHERE1
&&WHERE2
