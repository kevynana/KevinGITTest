-******************************************************************************
-* This program is used to LOAD data file
-******************************************************************************
-*SET &ECHO=ALL;
SET EDAPATH=d:\NewDBs; 
-RUN

-SET &FROMFILE=&&NEWPATH || 'DUMMY.FOC';
-SET &TOFILE = &&NEWPATH || &FILENAME || '.FOC';
-DOS COPY &FROMFILE &TOFILE
USE &TOFILE AS &FILENAME
END
-RUN

CREATE FILE &FILENAME
-RUN

REBUILD
REORG
LOAD
&FILENAME
Y
-RUN
