-* File pr_pop_rollup.fex
-DEFAULT &SCHED_OWNER='$*';
 
JOIN
 INNER PUSH_GROUPS.PUSH_GROUPS.SET_KEY IN PUSH_GROUPS TO UNIQUE
 RPT_SET.RPT_SET.SET_KEY IN RPT_SET AS J1
 END
 
DEFINE FILE PUSH_GROUPS
ROLLUPXX/A8=ROLLUP_CODE;
SEL/A1='N';
END
TABLE FILE PUSH_GROUPS
SUM FST.ROLLUPXX AS ROLL
    SEL
BY ROLLUPXX AS ROLL
WHERE SCHED_OWNER EQ '&SCHED_OWNER';
ON TABLE SET ASNAMES ON
ON TABLE SET HOLDLIST PRINTONLY
ON TABLE PCHOLD FORMAT XML
END
