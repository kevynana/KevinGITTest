        NET_TKT_CNT
        NEW_ADV_BOOK	
        NEW_ADV_PURCH
        NONREF_FLAG
        ORG_DST	
        PNR_LOCATOR	
        REF_EXCH_CNT	
        REFUSAL_DESC	
