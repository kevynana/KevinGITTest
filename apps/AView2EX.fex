-* Global Reporting JOIN
JOIN ADVP.TKT_DATE IN ADVP TO GLOBAL.DATE IN GLOBAL AS J1
END

DEFINE FILE ADVP
NAMEC/A30 = CNAME;

-******************************************************************************
-*                         Global Currency Conversion Defines
-******************************************************************************
CURR1X/D20.6 = 1 / USDPU ;

-* DEFINES ON PREVIOUS DEFINES
PAID_FAREX/D20.6 = FARE_PAID * CURR1X;
PAID_FARE1/D20.6C = PAID_FAREX;
DISSAVEX/D20.6 = DISSAVINGS * CURR1X;
SAVINGSX/D20.6 = SAVINGS * CURR1X;
DISSAVE1/D20.6 = DISSAVEX;
SAVINGS1/D20.6 = SAVINGSX;
END


