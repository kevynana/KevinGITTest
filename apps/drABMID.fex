DROP_CTY_NAME AS 'DROP,CITY'
BOOKINGS AS 'BKGS'
RENTAL_DAYS AS 'NMBR,DAYS'
RENTAL_AMT/D12.2CSM AS 'TTL,AMT'
COMPUTE AVGDR/D12.2CSM = IF RENTAL_AMT LT 0 AND RENTAL_DAYS LT 0 THEN
                        ((RENTAL_AMT/RENTAL_DAYS) * (-1)) ELSE
                      IF ((RENTAL_DAYS LT 0) AND (RENTAL_DAYS GT (-1))) THEN 0
                           ELSE (RENTAL_AMT/RENTAL_DAYS);
                           AS 'AVG,DLY,RTE'
