-RUN
-GOTO DVRCUSEEXIT;

-USECSingle
-SET &DB  = &DATAPATH | 'C_' || &&COMPNY || '.FOC AS CR_50';
-SET &UF1  = &DATAPATH | 'UC' || &&COMPNY || '.FOC AS CARUDID';

USE
&DB
&UF1
END
-RUN

-DVRCUSEEXIT