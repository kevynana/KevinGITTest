&&WHERE1
&&WHERE2
&&WHERE3

WHERE (TRN_DATE GE &&FROMDT.EVAL AND TRN_DATE LE &&TODT.EVAL) OR
      (TRN_DATE GE &&FXDATE.EVAL AND TRN_DATE LE &&TXDATE.EVAL)
WHERE AIRLINE_NAME EQ '&&ALN'
&&VOIDCHK
-INCLUDE RPTPARMS
ON TABLE HOLD AS RPTHOLD
END
