DEFINE FILE RPTHOLD 
CY/A1 = IF (TRN_DATE GE &&FROMDT.EVAL) AND (TRN_DATE LE &&TODT.EVAL) THEN
           'Y' ELSE 'N';
FLAG/A10 = IF CY EQ 'Y' THEN 'CURRENT' ELSE 'PRIOR';
END
-RUN

TABLE FILE RPTHOLD
SUM MRK_CNT FARE_PAID
BY PASSNGR_NAME
WHERE FLAG EQ 'CURRENT'
ON TABLE HOLD AS CRCUR1
END
-RUN

TABLE FILE CRCUR1
SUM MRK_CNT FARE_PAID
RANKED BY HIGHEST MRK_CNT NOPRINT
BY PASSNGR_NAME
ON TABLE HOLD AS CRCUR2
END
-RUN

DEFINE FILE CRCUR2
DRANK/D9=RANK;
ARANK/A11=FTOA(DRANK, '(D9)', 'A11');
ARANK1/A9=EDIT(ARANK, '$$999999999');
LIST/D9 = (LAST LIST + 1);
CRANKX/A11=FTOA(LIST, '(D9)', 'A11');
CRANK/A9=EDIT(CRANKX, '$$999999999');
RANK_VALUE/A9= IF LIST GT &&RANK_LIMIT THEN 'OTHER' ELSE CRANK;
RANK_LABEL/A33=IF RANK_VALUE NE 'OTHER' THEN PASSNGR_NAME
                  ELSE 'ALL OTHER';
END

TABLE FILE CRCUR2
SUM MRK_CNT FARE_PAID
BY RANK_VALUE
BY RANK_LABEL
ON TABLE HOLD AS CRCUR3
END
-RUN

TABLE FILE CRCUR3
SUM MRK_CNT FARE_PAID
BY RANK_VALUE
BY RANK_LABEL AS 'PASSNGR_NAME'
WHERE RANK_LABEL NE 'ALL OTHER'
ON TABLE HOLD AS CRCUR4 
END
-RUN

