  FARE_BAS	
  FARE_PAID	
  FLIGHT	
  LEVEL_DESC	
  LEVEL1	
  LEVEL2	
  LEVEL3	
  NET_TKT_CNT	
  NEW_SEG_DISC	
  REF_EXCH_CNT	
  REFUSAL_DESC	
  REFUSE_CODE	
  REFUSE_KEY        	
  RTE_CTY.CITY_NAME AS 'RTE_CTY_NM'	
  SEG_AMT	
  SEG_COUNT	
  SEG_DISCOUNT	
  SEG_LOWEST	
  SEG_NBR	
  SEG_TAX	
  STOPOVER
  TKT_NUM	
  TKT_PURCH	
  TKT_SORT	
  TKT_TYPE	
  TRN_DATE	
  VAL_AIRLINE	
  VASAVINGS	
  VTSAVINGS
  XARR_DT	
  XDPT_DT	
  XPSNGR_NM	
  XTRAN_DT	

-INCLUDE &AIRDATES

WHERE VOID_DATE EQ 0
-********************************************************************
