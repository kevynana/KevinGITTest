  LSTSVNGS
  DPT_TIME
  EMB_APT_NM
  FARE_PAID
  NET_TKT_CNT
  PASSNGR_NAME
  REFUSE_CODE
  RTE_APT_NM
  SEG_LOWEST
  TFARE
