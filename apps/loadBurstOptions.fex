TABLE FILE BRPT_QUE
PRINT
  RPT_INST.INST_KEY
  RPT_INST.ROLLUP_CODE
  RPT_INST.SET_KEY
  RPT_INST.RPT_GROUP
  RPT_INST.RPT_TYPE
  RPT_INST.RPT_STREAM
  RPT_INST.GLOBAL_PARM
  RPT_INST.USE_PARM
  RPT_INST.PAIR_DB
  RPT_INST.RPT_NAME
 
  SETTINGS.ID
  SETTINGS.RPT_TITLE1
  SETTINGS.RPT_TITLE2
  SETTINGS.RPT_FOOT1
  SETTINGS.RPT_FOOT2
  SETTINGS.STYLE1
  SETTINGS.STYLE2
  SETTINGS.STYLE3
  SETTINGS.SELECT_LEV
  SETTINGS.EXECUTE_FLAG
  SETTINGS.RUN_SEQ
  SETTINGS.CONTROL_ID
  SETTINGS.OUTPUT_FORMAT
  SETTINGS.SECURE_FLAG
  SETTINGS.DISTRB_FLAG
  SETTINGS.LOGO_1
  SETTINGS.LOGO_2
  SETTINGS.RPT_SERVER
 
  SETTINGS.FM_DATE
  SETTINGS.TO_DATE
  SETTINGS.FXDATE
  SETTINGS.TXDATE
  SETTINGS.CMFM_DATE
  SETTINGS.CMTO_DATE
  SETTINGS.PMFM_DATE
  SETTINGS.PMTO_DATE
  SETTINGS.CYFM_DATE
  SETTINGS.CYTO_DATE
  SETTINGS.PYFM_DATE
  SETTINGS.PYTO_DATE
  SETTINGS.BREAKBY
