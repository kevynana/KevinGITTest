-***************************************************************************
-* File:      SetBDBsForDrill.fex
-* Functions: This program is used to save the temporary B databases for
-*            drilldown reports into TTWEB directory. 
-*
-*            Called from drillable programs
-* Date:      02/28/2008
-*  03/31/2015  REJ  S-05681 Changes for Available Reports.  Adding code so that the
-*                           program will run in current Review and in Available Reports.
-*  06/26/2015  REJ  S-08389 Latest changes for ER5 Available Reports.
-****************************************************************************
-*-SET &NOWTM = HHMMSS ('A8');
-*-SET &NOWTM = EDIT(&NOWTM,'99$99$99');
-*-SET &NOWDT = &YYMD;
-*-SET &NOWDTTM = &NOWDT || '_' || &NOWTM;
-*-SET &IID = &&INSTID
-*-SET &&DTTMID = &NOWDTTM || '_' || &IID;


-SET &BRPT_QUE = &&WEB_PATH  || 'BRPT_QUE.FOC';
-SET &BRPTINST = &&WEB_PATH  || 'BRPTINST.FOC';
-SET &BRPT_SET = &&WEB_PATH  || 'BRPT_SET.FOC';
-SET &BROLLUP  = &&WEB_PATH  || 'BROLLUP.FOC';

-SET &&WEBPATH = '&&WEBSRV.EVAL' ||'\TTWEB\ ' || &&ROLLUP || '\ '||&&UNAME.EVAL|| '\';

-IF &&FROMAVRP EQ '' GOTO :SAVRP1;
-SET &BRPT_FLDS_HIERARCHY = &&WEB_PATH  || 'BRPT_FLDS_HIERARCHY.FOC';
-SET &BRPT_FLDS_MISC = &&WEB_PATH  || 'BRPT_FLDS_MISC.FOC';
-SET &BRPT_FLDS_PARM = &&WEB_PATH  || 'BRPT_FLDS_PARM.FOC';
-SET &BRPT_FLDS_PARM_VALUES = &&WEB_PATH  || 'BRPT_FLDS_PARM_VALUES.FOC';
-SET &BRPT_FLDS_UDID = &&WEB_PATH  || 'BRPT_FLDS_UDID.FOC';
-SET &BRPT_FLDS_TRVLSMRY = &&WEB_PATH  || 'BRPT_FLDS_TRVLSMRY.FOC';
-SET &BRPT_FLDS_DVR = &&WEB_PATH  || 'BRPT_FLDS_DVR.FOC';
-SET &BRPT_FLDS_HIDE_OPTIONS = &&WEB_PATH  || 'BRPT_FLDS_HIDE_OPTIONS.FOC';
-SET &BRPT_FLDS_PARM_REQUIRED = &&WEB_PATH  || 'BRPT_FLDS_PARM_REQUIRED.FOC';
-SET &BRPT_FLDS_PARM_VALUES_REQUIRED = &&WEB_PATH  || 'BRPT_FLDS_PARM_VALUES_REQUIRED.FOC';
 
 
 
-SET &BRPT_FLDS_HIERARCHY2 = &&WEBPATH ||&&DTTMID || 'BRPT_FLDS_HIERARCHY.FOC';
-SET &BRPT_FLDS_MISC2 = &&WEBPATH ||&&DTTMID || 'BRPT_FLDS_MISC.FOC';
-SET &BRPT_FLDS_PARM2 = &&WEBPATH ||&&DTTMID || 'BRPT_FLDS_PARM.FOC';
-SET &BRPT_FLDS_PARM_VALUES2 = &&WEBPATH ||&&DTTMID || 'BRPT_FLDS_PARM_VALUES.FOC';
-SET &BRPT_FLDS_UDID2 = &&WEBPATH ||&&DTTMID  || 'BRPT_FLDS_UDID.FOC';
-SET &BRPT_FLDS_TRVLSMRY2 = &&WEBPATH ||&&DTTMID  || 'BRPT_FLDS_TRVLSMRY.FOC';
-SET &BRPT_FLDS_DVR2 = &&WEBPATH ||&&DTTMID || 'BRPT_FLDS_DVR.FOC';
-SET &BRPT_FLDS_HIDE_OPTIONS2 = &&WEBPATH ||&&DTTMID || 'BRPT_FLDS_HIDE_OPTIONS.FOC';
-SET &BRPT_FLDS_PARM_REQUIRED2 = &&WEBPATH ||&&DTTMID  || 'BRPT_FLDS_PARM_REQUIRED.FOC';
-SET &BRPT_FLDS_PARM_VALUES_REQUIRED2 = &&WEBPATH ||&&DTTMID  || 'BRPT_FLDS_PARM_VALUES_REQUIRED.FOC';
 
 
 
 
 
COPY &BRPT_FLDS_HIERARCHY &BRPT_FLDS_HIERARCHY2
COPY &BRPT_FLDS_MISC &BRPT_FLDS_MISC2
COPY &BRPT_FLDS_PARM &BRPT_FLDS_PARM2
COPY &BRPT_FLDS_PARM_VALUES &BRPT_FLDS_PARM_VALUES2
COPY &BRPT_FLDS_UDID &BRPT_FLDS_UDID2
COPY &BRPT_FLDS_TRVLSMRY &BRPT_FLDS_TRVLSMRY2
COPY &BRPT_FLDS_DVR &BRPT_FLDS_DVR2
COPY &BRPT_FLDS_HIDE_OPTIONS &BRPT_FLDS_HIDE_OPTIONS2
COPY &BRPT_FLDS_PARM_REQUIRED &BRPT_FLDS_PARM_REQUIRED2
COPY &BRPT_FLDS_PARM_VALUES_REQUIRED &BRPT_FLDS_PARM_VALUES_REQUIRED2
 
 
 
 
-GOTO :SAVRP1_CONT;
-:SAVRP1
-SET &BRPTFLDS = &&WEB_PATH  || 'BRPTFLDS.FOC';
-SET &BRPTFLDS2 = &&WEBPATH ||&&DTTMID || 'BRPTFLDS.FOC';
COPY &BRPTFLDS &BRPTFLDS2
-:SAVRP1_CONT


-SET &BRPT_QUE2 = &&WEBPATH ||&&DTTMID || 'BRPT_QUE.FOC';
-SET &BRPTINST2  = &&WEBPATH ||&&DTTMID || 'BRPTINST.FOC';
-SET &BRPT_SET2 = &&WEBPATH ||&&DTTMID || 'BRPT_SET.FOC';
-SET &BROLLUP2   = &&WEBPATH ||&&DTTMID || 'BROLLUP.FOC';

COPY &BRPT_QUE &BRPT_QUE2
COPY &BRPTINST &BRPTINST2
COPY &BRPT_SET &BRPT_SET2
COPY &BROLLUP  &BROLLUP2
-RUN


