SET ASNAMES=ON

-* H1
TABLE FILE C1KIMB
PRINT CAR_KEY REFUSE_KEY
BY CAR_KEY NOPRINT
IF REFUSE_KEY OMITS 'KIMB'
IF BR_CL_IDX EQ 'KM999999'
ON TABLE HOLD AS DEBHOLD
END
-RUN



DEFINE FILE DEBHOLD
RFKEY1/A2 = EDIT(REFUSE_KEY, '99');
RFKEY2/A8 = EDIT(REFUSE_KEY, '$$99999999');
RFKEY4/A10 = RFKEY2 | RFKEY1;
-* RFKEY3/A10 = IF REFKEY EQ ' ' THEN ' ' ELSE RFKEY4;
END
TABLE FILE DEBHOLD
PRINT CAR_KEY
      RFKEY4 AS 'REFUSE_KEY'
BY CAR_KEY NOPRINT
BY RFKEY4 NOPRINT
ON TABLE HOLD AS DHOLD
END
-RUN


MODIFY FILE C1KIMB
FIXFORM FROM DHOLD
MATCH CAR_KEY
ON MATCH UPDATE REFKEY
ON NOMATCH REJECT
DATA ON DHOLD
END
-RUN


