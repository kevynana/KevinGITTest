DEFINE FILE ACLHHLD


IDAYS/P9CS = IF FLAG EQ 'AIR' THEN 0 ELSE
             IF FLAG EQ 'HTL2' THEN HIDAYS ELSE
             IF FLAG EQ 'CAR2' THEN CIDAYS ELSE 0;
 
             
TDAYS/P9CS = IF FLAG EQ 'AIR' THEN 0 ELSE
             IF FLAG EQ 'HTL2' THEN HTDAYS ELSE
             IF FLAG EQ 'CAR2' THEN CTDAYS ELSE 0;
             
OBKGS/P9CS = IF FLAG EQ 'AIR' THEN IBOOK_CNT ELSE 
             IF FLAG EQ 'HOTEL' THEN HIBOOK ELSE 
-*             IF FLAG EQ 'HTL2' THEN HIDAYS ELSE
             IF FLAG EQ 'CAR' THEN CIBOOK ELSE 
-*             IF FLAG EQ 'CAR2' THEN CIDAYS ELSE
             IF FLAG EQ 'LIMO' THEN LIBOOK ELSE 0;
     
         
             
TBKGS/P9CS = IF FLAG EQ 'AIR' THEN ABOOK_CNT ELSE          
             IF FLAG EQ 'HOTEL' THEN HTBOOK ELSE
-*             IF FLAG EQ 'HTL2' THEN HTDAYS ELSE
             IF FLAG EQ 'CAR' THEN CTBOOK ELSE
-*             IF FLAG EQ 'CAR2' THEN CTDAYS ELSE
             IF FLAG EQ 'LIMO' THEN LTBOOK ELSE 0;            
        
  

TBOOK/P9CS = IF FLAG EQ 'AIR' THEN NET_TKT_CNT ELSE 
             IF FLAG EQ 'HOTEL' THEN HBOOK ELSE 
-*             IF FLAG EQ 'HTL2' THEN TROOMS ELSE
             IF FLAG EQ 'CAR' THEN CBOOK ELSE
-*             IF FLAG EQ 'CAR2' THEN TOTCDAY ELSE
             IF FLAG EQ 'LIMO' THEN LBOOK ELSE 0;
             
TOTDN/P9CS = TROOMS + TOTCDAY;
             
             
TOTDN1/P9CS = IF FLAG EQ 'HTL2' THEN TOTDN ELSE
              IF FLAG EQ 'CAR2' THEN TOTDN ELSE 0;

             
OFARE/D12.2CS = IF FLAG EQ 'AIR' THEN IBOOK_FARE ELSE
                IF FLAG EQ 'HOTEL' THEN HI_AMT ELSE
                IF FLAG EQ 'HTL2' THEN HI_AMT ELSE 
                IF FLAG EQ 'CAR' THEN CI_AMT ELSE
                IF FLAG EQ 'CAR2' THEN CI_AMT ELSE
                IF FLAG EQ 'LIMO' THEN LI_AMT ELSE 0;
                 
TFARE/D12.2CS = IF FLAG EQ 'AIR' THEN ABOOK_FARE ELSE
                IF FLAG EQ 'HOTEL' THEN HT_AMT ELSE
                IF FLAG EQ 'HTL2' THEN HT_AMT ELSE
                IF FLAG EQ 'CAR' THEN CT_AMT ELSE
                IF FLAG EQ 'CAR2' THEN CT_AMT ELSE
                IF FLAG EQ 'LIMO' THEN LT_AMT ELSE 0;
                
TFARE1/D12.2CS = IF FLAG EQ 'AIR' THEN FARE_PAID ELSE
                IF FLAG EQ 'HOTEL' THEN HTL_AMT ELSE
                IF FLAG EQ 'HTL2' THEN HTL_AMT ELSE
                IF FLAG EQ 'CAR' THEN CAR_AMT ELSE
                IF FLAG EQ 'CAR2' THEN CAR_AMT ELSE
                IF FLAG EQ 'LIMO' THEN LIM_AMT ELSE 0; 
                
                
-* RBaker    S-16385 Merge rows 10 to 9 and 12 to 11/
FLAG = IF FLAG EQ 'HTL2' THEN 'HOTEL' ELSE 
       IF FLAG EQ 'CAR2' THEN 'CAR'   ELSE FLAG;
-*

FLAGA/A1 = 'A';
 
END       

TABLE FILE ACLHHLD
SUM OBKGS TBKGS TBOOK OFARE TFARE TFARE1 IDAYS TDAYS TROOMS TOTCDAY TOTDN1 
BY FLAGA    
BY LEVEL_DESC
BY FLAG 
ON TABLE HOLD AS ACLHHLD1
END
-RUN


DEFINE FILE ACLHHLD1
 NOWTOD/A8 WITH LEVEL_DESC = HHMMSS (NOWTOD); 
 FLAG1/A25 = IF FLAG EQ 'AIR'   THEN 'AIR TICKETS'   ELSE
             IF FLAG EQ 'HOTEL' THEN 'HOTEL'         ELSE
             IF FLAG EQ 'HTL2'  THEN 'HOTEL NIGHTS'  ELSE
             IF FLAG EQ 'CAR'   THEN 'CAR'           ELSE
             IF FLAG EQ 'CAR2'  THEN 'CAR DAYS'      ELSE
             IF FLAG EQ 'LIMO'  THEN 'LIMO' ELSE ' ';
            
FLAGD/A1 = IF FLAG EQ 'AIR'   THEN 'A' ELSE
           IF FLAG EQ 'HTL2'  THEN 'B' ELSE
           IF FLAG EQ 'HOTEL' THEN 'C' ELSE
           IF FLAG EQ 'CAR'   THEN 'E' ELSE
           IF FLAG EQ 'CAR2'  THEN 'D' ELSE
           IF FLAG EQ 'LIMO'  THEN 'F' ELSE ' ';
     
            
END

