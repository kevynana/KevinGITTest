-SET &&PAHDR = IF '&&OUTFMT.EVAL' EQ 'PDF' OR 'PPT' OR 'COM' THEN 'HEADERACLH' ELSE 'HDREXACH'; 
-SET &&PASTYS = IF '&&OUTFMT.EVAL' NE 'PDF' OR 'PPT' OR 'COM' THEN 'SMEXL' ELSE &&SMSTY;

TABLE FILE ACLHHLD1
HEADING
-INCLUDE &&PAHDR
"&&SUBHEAD"
-IF &&PAHDR EQ 'HDREXACH' GOTO NOSPC;
"</2"
-NOSPC;

SUM OBKGS AS 'ONLINE,BOOKING'
COMPUTE PCTIBK/D12S% = IF ((TBOOK LT 1) AND (TBOOK GT (-1))) THEN 0 ELSE ((OBKGS/TBOOK)*100); AS '% ONLINE,BOOKING'
TBKGS AS 'TRADITIONAL,BOOKING'
COMPUTE PCTABK/D12S% = IF ((TBOOK LT 1) AND (TBOOK GT (-1))) THEN 0 ELSE ((TBKGS/TBOOK)*100); AS '% TRADITIONAL'
COMPUTE AAVGTKT/D12.2CSM = IF ((TBKGS LT 1) AND (TBKGS GT (-1))) THEN 0 ELSE (TFARE/TBKGS); AS 'TRADITIONAL,AVERAGE,RATE'
COMPUTE IAVGTKT/D12.2CSM = IF ((OBKGS LT 1) AND (OBKGS GT (-1))) THEN 0 ELSE (OFARE/OBKGS); AS 'ONLINE,AVERAGE,RATE'
IDAYS AS 'ONLINE,DAYS/,NIGHTS'
COMPUTE IDYP/D12S% = IF TOTDN1 EQ 0 THEN 0 ELSE IDAYS/TOTDN1*100; AS '% ONLINE,DAYS/,NIGHTS'
TDAYS AS 'TRADITIONAL,DAYS/,NIGHTS'
COMPUTE TDYP/D12S% = IF TOTDN1 EQ 0 THEN 0 ELSE TDAYS/TOTDN1*100; AS '% TRADITIONAL,DAYS/,NIGHTS'
BY LEVEL_DESC NOPRINT
BY FLAGD NOPRINT
BY FLAG1 AS 'TYPE'

ON LEVEL_DESC RECOMPUTE MULTILINES AS 'TOTAL FOR'
-IF '&&OUTFMT.EVAL' NE 'PDF' GOTO NOSKPLINE;
ON LEVEL_DESC SKIP-LINE
-NOSKPLINE;
