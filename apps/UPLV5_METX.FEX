
-GOTO XXIT;
SET ASNAMES = ON 
-INCLUDE SETECHO



DEFINE FILE A9METX
XL01/A15 = IF ICODEX CONTAINS 'INT' THEN 'INT' ELSE
           IF ICODEX CONTAINS 'EXT' THEN 'EXT' ELSE ' ';
END
TABLE FILE A9METX
PRINT XL01 AS 'LEVEL5' 
BY AIR_KEY 
ON TABLE HOLD AS AIRHOLD
END
-RUN

MODIFY FILE A9METX
FIXFORM FROM AIRHOLD
MATCH AIR_KEY
ON MATCH UPDATE LEVEL5
ON NOMATCH REJECT
DATA ON AIRHOLD
END
-RUN

