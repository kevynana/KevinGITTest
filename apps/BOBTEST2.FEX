-INCLUDE SETECHO
-*SET TEMPERASE = OFF
TABLE FILE ROLLUP
ON TABLE SET PAGE-NUM OFF
PRINT  ROLLUP_CODE COMP
IF ROLLUP_CODE EQ 'CONG-R'
ON TABLE NOTOTAL
ON TABLE HOLD AS HOLDPASS
END
-RUN
-READ HOLDPASS &X.8 &COMP.6
-RUN
-SET &COMPID = 'D:\TNT\TTRACKER\DATA\HP' || &COMP || '.FOC AS HIER50';
USE ADD
&COMPID.EVAL
END
-RUN
 
CREATE FILE CONTROL
-RUN
 
-TYPE Before first Control MODIFY
MODIFY FILE CONTROL
FIXFORM ROLLUP_CODE/A8
COMPUTE CONTROL_ID/A3  =  'PGM';
COMPUTE USER_ID/A20 = 'steve';
COMPUTE SECURITY_LEVEL/I1 = 9;
COMPUTE WEB_RUN/A1 = 'Y';
MATCH CONTROL_ID
ON MATCH REJECT
ON NOMATCH INCLUDE
DATA ON HOLDPASS
END
-RUN


? PATH
-RUN

