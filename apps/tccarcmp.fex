TABLE FILE TCCARTRN
SUM NEW_RENTL_AMT
    GROSS_BOOK
    ROLLCD AS 'ROLL1'
    ROLLCD AS 'ROLLUP_CODE'
BY ROLLCD
ON TABLE HOLD AS HOLDTWO
END
-RUN
 
 
TABLE FILE HOLDTWO
PRINT NEW_RENTL_AMT
      GROSS_BOOK
      ROLL1
BY ROLLUP_CODE
ON TABLE HOLD AS HOLD3
END
-RUN
 
 
TABLE FILE TCLIMTRN
SUM NEW_RENTL_AMT AS 'LIM_AMT'
    GROSS_BOOK AS 'LBOOK'
    ROLLCD AS 'ROLL1'
    ROLLCD AS 'ROLLUP_CODE'
BY ROLLCD
ON TABLE HOLD AS HOLD4
END
 
MATCH FILE HOLD3
PRINT NEW_RENTL_AMT
      GROSS_BOOK
BY ROLLUP_CODE
BY ROLL1
ON TABLE HOLD
RUN
FILE HOLD4
SUM LIM_AMT
    LBOOK
BY ROLLUP_CODE
BY ROLL1
AFTER MATCH HOLD AS MATCHONE OLD-OR-NEW
END
-RUN
 
 
 
 
DEFINE FILE TCCAR400
CARAMT/D14.2 = CAR_AMT;
BKGS/D9 = BOOKINGS;
END
TABLE FILE TCCAR400
PRINT CARAMT AS 'CAR_AMT'
      BKGS AS 'BOOKINGS'
BY ROLLUP_CODE
ON TABLE HOLD AS CARHOLD4
END
-RUN
 
 
 
MATCH FILE MATCHONE
PRINT NEW_RENTL_AMT
      GROSS_BOOK
      ROLL1
      LIM_AMT
      LBOOK
BY ROLLUP_CODE
ON TABLE HOLD
RUN
FILE CARHOLD4
SUM CAR_AMT
    BOOKINGS
 
BY ROLLUP_CODE
AFTER MATCH HOLD AS XONE OLD-OR-NEW
END
-RUN
 
 
 
 
 
JOIN XONE.ROLLUP_CODE IN XONE TO ROLLUP.ROLLUP_CODE IN ROLLUP AS J1
END
-RUN
 
TABLE FILE XONE
PRINT NEW_RENTL_AMT
      GROSS_BOOK
      CAR_AMT
      BOOKINGS
      ROLL1
      LIM_AMT
      LBOOK
      RPT_CYCLE
BY ROLLUP_CODE
ON TABLE HOLD AS CARHOLD5
END
-RUN
 
 
 
DEFINE FILE CARHOLD5
 
FLAG/A1 = IF RPT_CYCLE EQ ' ' THEN 'Z' ELSE
           IF RPT_CYCLE EQ 'W' THEN 'A' ELSE 'B';
END
TABLE FILE CARHOLD5
SUM CAR_AMT
    BOOKINGS
    ROLL1
    NEW_RENTL_AMT
    GROSS_BOOK
    LIM_AMT
    LBOOK
BY FLAG
BY ROLLUP_CODE
ON TABLE HOLD AS TRANTWO
END
-RUN
 
 
 
