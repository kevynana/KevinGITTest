FILEDEF TCTRNCAR DISK &&DATASRV.EVAL\TNT\TTRACKER\TEMP\TCTRNCAR.TXT
 
SET ASNAMES=ON
 
 
TABLE FILE TCTRNCAR
PRINT ROLLUP_CODE/A8
      CAR_AMT/D11.2
      BOOKINGS/D11
BY ROLLUP NOPRINT
WHERE ROLLUP NE ' '
ON TABLE HOLD AS HOLDONE
END
-RUN
 
 
 
MODIFY FILE TCCAR400
FIXFORM FROM HOLDONE
DATA ON HOLDONE
END
-RUN
 
 
 
CREATE FILE TCCARTRN
-RUN
CREATE FILE TCLIMTRN
