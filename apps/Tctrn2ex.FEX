MATCH FILE TC_TRAN2
PRINT AGENT_NUM DOC_TYPE DOD_TYPE EMBARK EX_FLAG EXCHG_SALE RF_FLAG SEG_AMT
  SEG_COM SEG_COUNT SEG_NBR SEG_TAX TKT_SORT TRN_DATE TKT_NUM TICKET_BRANCH DOD_DESC
  VOID_DATE VOID_STATUS TF_TKT
BY AIR_KEY
ON TABLE HOLD
RUN 
FILE TCUDH2
