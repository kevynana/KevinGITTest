DEFINE FILE HR_50
CURRENT/A1 = IF INVOICE_DATE GE &&FROMDT.EVAL AND INVOICE_DATE LE &&TODT.EVAL THEN 'Y' ELSE 'N';
CITY_ST/A35 = PROP_CITY||(', '| PROP_STATE);
IN_DATE/MDYY = CHKIN_DATE;
INV_DT/YYMD = INVOICE_DATE;
NBR_DAYS/D12 = NUM_DAYS;
NBR_ROOMS/D12 = NUM_ROOMS;
NEW_ROOM_RATE/D15.2 = ROOM_RATE;
NEW_TOTAL_AMT/D15.2 = TOTAL_AMT;
 
NNROOMS/D12CS = IF (NUM_DAYS LT 0) AND
                 (NUM_ROOMS LT 0) THEN
                 (NUM_DAYS * NUM_ROOMS) * (-1) ELSE
                 (NUM_DAYS * NUM_ROOMS);
CDAY/D12CS = IF CURRENT EQ 'Y' THEN NNROOMS ELSE 0;
 
OUT_DATE/MDYY = CHKOUT_DATE;
CHAIN/A30 = LCWORD(30, CHAIN_NAME, CHAIN);
END
-RUN
 
TABLEF FILE HR_50
PRINT CDAY
  CITY_ST
  CHAIN_NAME
  CURR_DATE
  IN_DATE
  INV_DT
  INVOICE_DATE
  NBR_DAYS
  NBR_ROOMS
  NEW_TOTAL_AMT
  NNROOMS
  OUT_DATE
  PASSNGR_NAME
  PROP_NAME
-***************************************************************
-* Where statements to select data specified in user profile
-***************************************************************
WHERE INVOICE_DATE GE &&FROMDT.EVAL AND INVOICE_DATE LE &&TODT.EVAL OR
      INVOICE_DATE GE &&FXDATE.EVAL AND INVOICE_DATE LE &&TXDATE.EVAL
&&WHERE1
&&WHERE2
&&WHERE3
 
-INCLUDE RPTPARMS
ON TABLE HOLD AS RPTHOLD
END
-RUN
 
TABLE FILE RPTHOLD
SUM CDAY NNROOMS
BY CHAIN_NAME
ON TABLE HOLD AS OVH1
END
-RUN
 
TABLE FILE OVH1
PRINT *
RANKED BY HIGHEST CDAY NOPRINT
ON TABLE HOLD AS OVH2
END
-RUN
 
DEFINE FILE OVH2
LIST/D9 = IF CHAIN_NAME NE LAST CHAIN_NAME THEN (LAST LIST + 1) ELSE LAST LIST;
DRANK/D9 = RANK;
ARANK/A11 = FTOA(DRANK, '(D9)', ARANK);
ARANK1/A9 = EDIT(ARANK, '$$999999999');
RANK_VALUE/A9 = IF LIST GT 3 THEN 'OTHER' ELSE ARANK1;
RANK_LABEL/A30 = IF RANK_VALUE NE 'OTHER' THEN CHAIN_NAME ELSE 'ALL OTHER';
END
-RUN
 
TABLE FILE OVH2
SUM CDAY
BY CHAIN_NAME
WHERE RANK_VALUE EQ 'OTHER'
ON TABLE HOLD AS OVH3
END
-RUN
 
