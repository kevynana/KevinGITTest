DEFINE FILE HR_50
CITY_ST/A35 = PROP_CITY||(', '| PROP_STATE);
IN_DATE/MDYY = CHKIN_DATE;
INV_DT/YYMD = INVOICE_DATE;
NBR_DAYS/D12 = NUM_DAYS;
NBR_ROOMS/D12 = NUM_ROOMS;
NEW_ROOM_RATE/D15.2 = ROOM_RATE;
NEW_TOTAL_AMT/D15.2 = TOTAL_AMT;
 
NNROOMS/D12 = IF (NUM_DAYS LT 0) AND
                 (NUM_ROOMS LT 0) THEN
                 (NUM_DAYS * NUM_ROOMS) * (-1) ELSE
                 (NUM_DAYS * NUM_ROOMS);
 
OUT_DATE/MDYY = CHKOUT_DATE;
CHAIN/A30 = LCWORD(30, CHAIN_NAME, CHAIN);
END
-RUN
 
