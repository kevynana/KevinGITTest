DEFINE FILE RPTHOLD1
-******************************************************************************
-*                         Global Currency Conversion Defines
-******************************************************************************
  CURR1X/D20.6 = 1 / USDPU ;
 
-* DEFINES ON PREVIOUS DEFINES
  HTLAMT/D20.6 = NEW_TOTAL_AMT * CURR1X;
 
-* DEFINES ON PREVIOUS DEFINES
 
  HTLAMTX/D20.6 = HTLAMT;
 
-* DEFINES ON PREVIOUSLY DEFINED DEFINES
 
  HTLAMT1/D15.2CS = HTLAMTX;
  NAMEC1/A30 = CNAME;
  NAMEC/A30 = IF COUNTRYCODE EQ 'USD' THEN ' ' ELSE NAMEC1;
  NOWTOD/A8 WITH NBR_DAYS = HHMMSS (NOWTOD);
END
-RUN

-SET &&PAHDR  = IF '&&OUTFMT.EVAL' EQ 'PDF' OR 'PPT' OR 'COM' THEN 'HEADAIR' ELSE 'HDREXLS'; 
-SET &&PASTYD = IF '&&OUTFMT.EVAL' NE 'PDF' OR 'PPT' OR 'COM' THEN 'DTEXL' ELSE &&DTSTY;
-RUN

 
TABLE FILE RPTHOLD1
-INCLUDE &&PAHDR
"&&SUBHEAD"
-IF &&PAHDR EQ 'HDREXLS' GOTO NOSPC;
"</2"
-NOSPC;
-*
-*******************************************************************
-* Sum statement with fields passed from the user profile database
-*******************************************************************
-*
SUM
NNROOMS AS 'NUMBER,NIGHTS'
NBR_ROOMS AS 'NUMBER,ROOMS'
HTLAMT1 AS 'TOTAL,AMOUNT'
COMPUTE AVGDR/D12.2CS = IF HTLAMT1 LT 0 AND NNROOMS LT 0
 THEN ((HTLAMT1/NNROOMS) * (-1))
 ELSE HTLAMT1/NNROOMS; AS 'AVERAGE,DAILY,RATE'
 
-*
-******************************************************************
-* By statements (one per field)
-******************************************************************
-*
BY PASSNGR_NAME AS 'PASSENGER NAME'
BY PROP_NAME AS 'HOTEL'
BY CITY_ST AS 'CITY/STATE'
BY IN_DATE AS 'CHECKIN,DATE'
BY OUT_DATE AS 'CHECKOUT,DATE'
-*
WHERE GLOBAL.COUNTRYCODE EQ '&&CURRENCY'
-******************************************************************
