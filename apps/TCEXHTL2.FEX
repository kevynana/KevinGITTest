E_STACK(3).DOM_CURR = E_STACK(3).DOM_CURR
                       + TSHTL1_STACK(CNT1).CDM1HTL_AMT; 
                       
E_STACK(3).DOM_PRIOR = E_STACK(3).DOM_PRIOR
