JOIN SET_KEY IN RPT_INST TO SET_KEY IN RPT_SET AS J1
END

TABLE FILE RPT_SET
PRINT SET_NAME 
BY SET_NAME
WHERE ROLLUP EQ 'STAPLE-R'
WHERE USER_NAME EQ ' '
-* ON TABLE HOLD
END
-EXIT

MODIFY FILE RPT_INST
FIXFORM INST_KEY/A72
MATCH INST_KEY
ON MATCH DELETE
ON NOMATCH REJECT
DATA ON HOLD
END

-EXIT







