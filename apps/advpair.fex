ON TABLE HOLD
RUN
FILE HOLDTWO
SUM CFARE
    CNTKT
    PFARE
    PNTKT
BY LEVEL_DESC
AFTER MATCH HOLD AS XTHREE OLD-OR-NEW
END
-RUN




DEFINE FILE XTHREE
  PCTCFARE/D8.1S =   IF ((CFARE LT 1) AND (CFARE GT (-1))) THEN 0
                    ELSE ((CURFARE/CFARE)*100);
  PCTCNTKT/D8.4S = IF ((CNTKT LT 1) AND (CNTKT GT (-1))) THEN 0 
                    ELSE ((CURNTKT/CNTKT)*100);
  PCTPFARE/D8.1S =   IF ((PFARE LT 1) AND (PFARE GT (-1))) THEN 0
                    ELSE ((PRFARE/PFARE)*100);
  PCTPNTKT/D8.4S = IF ((PNTKT LT 1) AND (PNTKT GT (-1))) THEN 0 
                    ELSE ((PRNTKT/PNTKT)*100);
END

TABLE FILE XTHREE
  SUM CURFARE
      CURNTKT
      PRFARE
      PRNTKT
      PCTCFARE
      PCTCNTKT
      PCTPFARE
      PCTPNTKT         
  COMPUTE CAVG/D9.2S= IF ((CURNTKT LT 1) AND (CURNTKT GT (-1))) THEN 0
                     ELSE (CURFARE/CURNTKT);
  COMPUTE PAVG/D9.2S= IF ((PRNTKT LT 1) AND (PRNTKT GT (-1))) THEN 0
                     ELSE (PRFARE/PRNTKT);
BY MX_SORT
BY LEVEL_DESC
BY MX_ADV_PUR
ON TABLE HOLD AS XFOUR FORMAT FOCUS
END
-RUN



-* TOTALS EXCLUDING 0 - 6 ADV PUR DAYS

TABLE FILE XFOUR
  SUM PCTCFARE
      PCTCNTKT
      PCTPFARE
      PCTPNTKT   
BY LEVEL_DESC
WHERE MX_ADV_PUR NE '0 - 6'
ON TABLE HOLD AS HOLDADV 
END




