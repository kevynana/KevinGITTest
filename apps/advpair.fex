DEFINE FILE &&RPT_HOLD
CURRENT/A1 = IF TRN_DATE GE &&FROMDT.EVAL AND
                TRN_DATE LE &&TODT.EVAL THEN 'Y' ELSE 'N';
PRIOR/A1 =  IF TRN_DATE GE &&FXDATE.EVAL AND
               TRN_DATE LE &&TXDATE.EVAL THEN 'Y' ELSE 'N';
CURFARE/D12.2CS = IF CURRENT EQ 'Y' THEN FARE_PAID ELSE 0;
CURNTKT/P8CS = IF CURRENT EQ 'Y' THEN NET_TKT_CNT ELSE 0;
PRFARE/D12.2CS = IF PRIOR EQ 'Y' THEN FARE_PAID ELSE 0;
PRNTKT/P8CS = IF PRIOR EQ 'Y' THEN NET_TKT_CNT ELSE 0;
END
-RUN


TABLE FILE &&RPT_HOLD
PRINT
        BOOK_DT	
        BR_CL_IDX
        CURRENT
        CURFARE
        CURNTKT	
        DOC_TYPE
        EMB_APT_CD
 	RTE_APT_CD
        AIRLINE
        GRP_ADV_PUR	
        LEVEL_DESC	
        LEVEL1	
        LEVEL2	
        LEVEL3
        MC_ADV_BOOK	
        MC_DAYS
        MRK_TKT_CNT	
        MX_ADV_PUR
        MX_DAYS
        MX_SORT
        NEW_ADV_BOOK	
        NEW_ADV_PURCH	
        PRIOR
        PRFARE
        PRNTKT	
        SEG_AMT	
        SEG_TAX	
        TICKET_BRANCH	
        TKT_NUM	
        TKT_SORT	
        TKT_TYPE	
        TRN_DATE	
        UP_ADV_PUR
        UP_DAYS
        XTRAN_DT
BY LEVEL_DESC	
ON TABLE HOLD AS HOLDONE 
END
-RUN



TABLE FILE HOLDONE
SUM CURFARE AS 'CFARE'
    PRFARE AS 'PFARE'
    CURNTKT AS 'CNTKT'
    PRNTKT AS 'PNTKT'
BY LEVEL_DESC
ON TABLE HOLD AS HOLDTWO
END 
-RUN

