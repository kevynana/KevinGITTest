-IF &&CPFLAG EQ 'CM' THEN GOTO CURMONTH
-  ELSE IF &&CPFLAG EQ 'PM' THEN GOTO PRRMONTH
-  ELSE IF &&CPFLAG EQ 'CY' THEN GOTO CURYEAR
-  ELSE IF &&CPFLAG EQ 'PY' THEN GOTO PRRYEAR
-  ELSE GOTO CURMONTH; 

-CURMONTH
-SET &&DCHK='WHERE INV_DT GE '&&CMFMDT.EVAL' AND INV_DT LE '&&CMTODT.EVAL'';
