-TYPE Before Extract
SET ASNAMES = ON
-SET HOLDATTR = ON;
-RUN

DEFINE FILE &&EXTRACT
BOOKINGS/D8 = IF NUM_DAYS GT 0 THEN 1 ELSE IF NUM_DAYS LT 0 THEN (-1) ELSE 0;

INTL_DOM/A1 =IF (PICK_CTY.INTL_DOM EQ 'I') THEN 'I' ELSE 'D';
-* INTL_DOM/A1 =IF (PICK_CTY.COUNTRY_CODE EQ 'US' OR ' ') THEN 'D' ELSE 'I';
-* INTL_DOM/A1 =IF (AIRPORT.INTL_DOM EQ 'D' OR ' ') THEN 'D' ELSE 'I';
DROP_OFF_DT/MDYY = DROP_DATE; 
PICK_UP_DT/MDYY = PICKUP_DATE;
NBR_DAYS/P12 = IF NUM_DAYS LT 0 AND NUM_CARS LT 0
                THEN ((NUM_DAYS * NUM_CARS) * (-1)) ELSE
                NUM_DAYS * NUM_CARS;         
INV_DT/YYMD = INVOICE_DATE;
-INCLUDE TRANTYPE  
END
-RUN

TABLEF FILE &&EXTRACT
PRINT 
  LIM_MAIN.CAR_KEY
  BOOKINGS
  CAR_KEY     
  CAR_TYPE    
  DAILY_RATE  
  DROP_CTY.CITY_NAME AS 'DROP_CTY_NAME'       
  DROP_OFF_DT 
  INVOICE_DATE  
  NBR_DAYS    
  PASSNGR_NAME        
  PICK_CTY.CITY_NAME AS 'PICK_CTY_NAME'	
  PICK_UP_DT   
  REFUSE_KEY  
  RENTAL_AMT	
  VENDOR_NAME
