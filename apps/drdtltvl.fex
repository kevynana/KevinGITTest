-PRRMONTH
-SET &&DCHK='WHERE INV_DT GE '&&PMFMDT.EVAL' AND INV_DT LE '&&PMTODT.EVAL'';
-SET &&FROMDT = &&PMFMDT;
-SET &&TODT = &&PMTODT;
-GOTO ENDDATE

-CURYEAR
-SET &&DCHK='WHERE INV_DT GE '&&CYFMDT.EVAL' AND INV_DT LE '&&CYTODT.EVAL'';
-SET &&FROMDT = &&CYFMDT;
-SET &&TODT = &&CYTODT;
-GOTO ENDDATE

-PRRYEAR
-SET &&DCHK='WHERE INV_DT GE '&&PYFMDT.EVAL' AND INV_DT LE '&&PYTODT.EVAL'';
-SET &&FROMDT = &&PYFMDT;
-SET &&TODT = &&PYTODT;
-GOTO ENDDATE

