-INCLUDE SETECHO
-DEFAULT &&FROMER5 = 'N'; 
CREATE FILE TC_NHD
-RUN
 
 
-INCLUDE TCGETROLL
-RUN
 
 
-SET &CNT = 1;
-REPEAT Loop2 &TOTROLLS TIMES
-SET &&ROLL = &ROLL.&CNT;
 
 
EX TCAIRDB
-RUN
 
-IF &FOCERRNUM EQ 36 THEN GOTO IncLoop2;
 
EX TCANHDEX
-RUN
 
 
-IncLoop2
-SET &CNT = &CNT + 1;
-Loop2
 
 
-INCLUDE SetParmtime
-RUN
 
-IF &&FROMAVRP NE 'Y' GOTO :AVSF0;
-INCLUDE AVRP_SUBFOOT
-:AVSF0
 
-******************************************************************
-* GET THE FINAL REPORT
-******************************************************************
 
TABLE FILE TC_NHD
PRINT HTL_ATTCH/D9C HTL_NOATTCH/D9C TOT_ATTCH/D9C
BY ROLLCD
ON TABLE HOLD AS HOLD2
END
-RUN



 
 
-SET &&RPTSUF = 'SMY';
-INCLUDE FDEFRPTS
-RUN
 
-TYPE BEFORE DEFINES
 
DEFINE FILE HOLD2
NOWTOD/A8 WITH ROLLCD = HHMMSS (NOWTOD);
END
 
-SET &&PAHDR = IF '&&OUTFMT.EVAL' EQ 'PDF' OR 'PPT' OR 'COM' THEN 'HEADER' ELSE 'HDREXLS';
-SET &&PASTYS = IF '&&OUTFMT.EVAL' NE 'PDF' OR 'PPT' OR 'COM' THEN 'SMEXL' ELSE &&SMSTY;
 
TABLE FILE HOLD2
-INCLUDE &&PAHDR
   &&S_SUBJ1
